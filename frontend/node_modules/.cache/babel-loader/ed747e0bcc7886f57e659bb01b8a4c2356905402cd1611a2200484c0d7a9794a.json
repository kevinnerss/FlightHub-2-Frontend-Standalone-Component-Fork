{"ast":null,"code":"import alarmApi from '../api/alarmApi.js';\nimport waylineApi from '../api/waylineApi.js';\nexport default {\n  name: 'AlarmList',\n  data() {\n    return {\n      alarms: [],\n      waylines: [],\n      filteredAlarms: [],\n      searchQuery: '',\n      statusFilter: '',\n      waylineIdFilter: '',\n      loading: false,\n      currentPage: 1,\n      pageSize: 10,\n      totalAlarms: 0,\n      showStatusDialog: false,\n      showDetailDialog: false,\n      currentAlarm: null,\n      newAlarmStatus: ''\n    };\n  },\n  mounted() {\n    this.loadAlarms();\n    this.loadWaylines();\n  },\n  methods: {\n    async loadAlarms() {\n      this.loading = true;\n      try {\n        const params = {\n          page: this.currentPage,\n          page_size: this.pageSize\n        };\n        if (this.searchQuery) params.search = this.searchQuery;\n        if (this.statusFilter) params.status = this.statusFilter;\n        if (this.waylineIdFilter) params.wayline_id = this.waylineIdFilter;\n        const response = await alarmApi.getAlarms(params);\n        this.alarms = response.results || response;\n        this.totalAlarms = response.count || this.alarms.length;\n        this.filteredAlarms = this.alarms;\n      } catch (error) {\n        console.error('Âä†ËΩΩÂëäË≠¶Â§±Ë¥•:', error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async loadWaylines() {\n      try {\n        const response = await waylineApi.getWaylines();\n        this.waylines = response.results || response;\n      } catch (error) {\n        console.error('Âä†ËΩΩËà™Á∫øÂ§±Ë¥•:', error);\n      }\n    },\n    handleSearch() {\n      clearTimeout(this.searchTimeout);\n      this.searchTimeout = setTimeout(() => {\n        this.currentPage = 1;\n        this.loadAlarms();\n      }, 300);\n    },\n    handlePageChange(page) {\n      if (page < 1 || page > Math.ceil(this.totalAlarms / this.pageSize)) return;\n      this.currentPage = page;\n      this.loadAlarms();\n    },\n    formatDate(dateString) {\n      if (!dateString) return '--';\n      const date = new Date(dateString);\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    getWaylineId(alarm) {\n      return alarm.wayline_details?.wayline_id || alarm.wayline_id || '--';\n    },\n    getWaylineName(alarm) {\n      return alarm.wayline_details?.name || alarm.wayline?.name || 'Êú™Áü•Ëà™Á∫ø';\n    },\n    getStatusText(status) {\n      const statusMap = {\n        'PENDING': 'ÂæÖÂ§ÑÁêÜ',\n        'PROCESSING': 'Â§ÑÁêÜ‰∏≠',\n        'COMPLETED': 'Â∑≤ÂÆåÊàê',\n        'IGNORED': 'Â∑≤ÂøΩÁï•'\n      };\n      return statusMap[status] || status;\n    },\n    getCategoryClass(category) {\n      if (!category) return '';\n      return `category-${category.toLowerCase().replace(/\\s+/g, '-')}`;\n    },\n    viewAlarmDetail(alarm) {\n      this.currentAlarm = alarm;\n      this.showDetailDialog = true;\n    },\n    updateAlarmStatus(alarm) {\n      this.currentAlarm = alarm;\n      this.newAlarmStatus = alarm.status;\n      this.showStatusDialog = true;\n    },\n    async confirmStatusUpdate() {\n      try {\n        await alarmApi.patchAlarm(this.currentAlarm.id, {\n          status: this.newAlarmStatus\n        });\n        this.showStatusDialog = false;\n        this.loadAlarms();\n      } catch (error) {\n        console.error('Êõ¥Êñ∞Áä∂ÊÄÅÂ§±Ë¥•:', error);\n      }\n    },\n    async deleteAlarm(id) {\n      if (!confirm('Á°ÆÂÆöË¶ÅÂà†Èô§ËøôÊù°ÂëäË≠¶ÂêóÔºü')) return;\n      try {\n        await alarmApi.deleteAlarm(id);\n        this.loadAlarms();\n      } catch (error) {\n        console.error('Âà†Èô§ÂëäË≠¶Â§±Ë¥•:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["alarmApi","waylineApi","name","data","alarms","waylines","filteredAlarms","searchQuery","statusFilter","waylineIdFilter","loading","currentPage","pageSize","totalAlarms","showStatusDialog","showDetailDialog","currentAlarm","newAlarmStatus","mounted","loadAlarms","loadWaylines","methods","params","page","page_size","search","status","wayline_id","response","getAlarms","results","count","length","error","console","getWaylines","handleSearch","clearTimeout","searchTimeout","setTimeout","handlePageChange","Math","ceil","formatDate","dateString","date","Date","toLocaleString","year","month","day","hour","minute","getWaylineId","alarm","wayline_details","getWaylineName","wayline","getStatusText","statusMap","getCategoryClass","category","toLowerCase","replace","viewAlarmDetail","updateAlarmStatus","confirmStatusUpdate","patchAlarm","id","deleteAlarm","confirm"],"sources":["D:\\projects\\FlightHub-2-Frontend-Standalone-Component\\frontend\\src\\components\\AlarmList.vue"],"sourcesContent":["<template>\r\n  <div class=\"alarm-list-premium\">\r\n    <!-- Â§¥ÈÉ® -->\r\n    <div class=\"list-header\">\r\n      <h2 class=\"list-title\">ÂëäË≠¶‰ø°ÊÅØÂàóË°®</h2>\r\n    </div>\r\n    \r\n    <!-- ÊêúÁ¥¢ÂíåÁ≠õÈÄâ -->\r\n    <div class=\"search-filters-premium\">\r\n      <div class=\"search-wrapper\">\r\n        <div class=\"search-icon\">üîç</div>\r\n        <input \r\n          v-model=\"searchQuery\"\r\n          @input=\"handleSearch\"\r\n          placeholder=\"ÊêúÁ¥¢ÂëäË≠¶ÂÜÖÂÆπ...\"\r\n          class=\"search-input\"\r\n        />\r\n      </div>\r\n      \r\n      <select v-model=\"statusFilter\" @change=\"loadAlarms\" class=\"filter-select\">\r\n        <option value=\"\">ÂÖ®ÈÉ®Áä∂ÊÄÅ</option>\r\n        <option value=\"PENDING\">ÂæÖÂ§ÑÁêÜ</option>\r\n        <option value=\"PROCESSING\">Â§ÑÁêÜ‰∏≠</option>\r\n        <option value=\"COMPLETED\">Â∑≤ÂÆåÊàê</option>\r\n        <option value=\"IGNORED\">Â∑≤ÂøΩÁï•</option>\r\n      </select>\r\n      \r\n      <select v-model=\"waylineIdFilter\" @change=\"loadAlarms\" class=\"filter-select\">\r\n        <option value=\"\">ÂÖ®ÈÉ®Ëà™Á∫ø</option>\r\n        <option v-for=\"wayline in waylines\" :key=\"wayline.id\" :value=\"wayline.wayline_id\">\r\n          {{ wayline.wayline_id }} - {{ wayline.name }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n    \r\n    <!-- ÂëäË≠¶Ë°®Ê†º -->\r\n    <div class=\"table-container\">\r\n      <div v-if=\"loading\" class=\"loading-state\">\r\n        <div class=\"loading-spinner\"></div>\r\n        <p>Âä†ËΩΩ‰∏≠...</p>\r\n      </div>\r\n      \r\n      <table v-else class=\"alarm-table\">\r\n        <thead>\r\n          <tr>\r\n            <th width=\"80\">ID</th>\r\n            <th width=\"100\">Ëà™Á∫øID</th>\r\n            <th width=\"150\">Ëà™Á∫øÂêçÁß∞</th>\r\n            <th width=\"180\">Êó∂Èó¥</th>\r\n            <th width=\"120\">Á±ªÂûã</th>\r\n            <th>ÊèèËø∞</th>\r\n            <th width=\"150\">‰ΩçÁΩÆ</th>\r\n            <th width=\"100\">Áä∂ÊÄÅ</th>\r\n            <th width=\"200\">Êìç‰Ωú</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-if=\"filteredAlarms.length === 0\">\r\n            <td colspan=\"9\" class=\"empty-row\">ÊöÇÊó†ÂëäË≠¶Êï∞ÊçÆ</td>\r\n          </tr>\r\n          <tr v-for=\"alarm in filteredAlarms\" :key=\"alarm.id\" class=\"alarm-row\">\r\n            <td>\r\n              <span class=\"id-badge\">{{ alarm.id }}</span>\r\n            </td>\r\n            <td>{{ getWaylineId(alarm) }}</td>\r\n            <td>{{ getWaylineName(alarm) }}</td>\r\n            <td>\r\n              <span class=\"datetime-text\">{{ formatDate(alarm.created_at) }}</span>\r\n            </td>\r\n            <td>\r\n              <span class=\"category-badge\" :class=\"getCategoryClass(alarm.category_name)\">\r\n                {{ alarm.category_name || 'Êú™ÂàÜÁ±ª' }}\r\n              </span>\r\n            </td>\r\n            <td class=\"description-cell\">{{ alarm.content }}</td>\r\n            <td>ÂùêÊ†á({{ alarm.latitude || '--' }}, {{ alarm.longitude || '--' }})</td>\r\n            <td>\r\n              <span class=\"status-badge\" :class=\"`status-${alarm.status.toLowerCase()}`\">\r\n                {{ getStatusText(alarm.status) }}\r\n              </span>\r\n            </td>\r\n            <td>\r\n              <div class=\"action-buttons\">\r\n                <button @click=\"viewAlarmDetail(alarm)\" class=\"action-btn view-btn\">\r\n                  Êü•Áúã\r\n                </button>\r\n                <button @click=\"updateAlarmStatus(alarm)\" class=\"action-btn update-btn\">\r\n                  Êõ¥Êñ∞\r\n                </button>\r\n                <button @click=\"deleteAlarm(alarm.id)\" class=\"action-btn delete-btn\">\r\n                  Âà†Èô§\r\n                </button>\r\n              </div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    \r\n    <!-- ÂàÜÈ°µÂô® -->\r\n    <div class=\"pagination-premium\">\r\n      <div class=\"pagination-info\">\r\n        ÊòæÁ§∫ {{ Math.min((currentPage - 1) * pageSize + 1, totalAlarms) }} - {{ Math.min(currentPage * pageSize, totalAlarms) }} Êù°ÔºåÂÖ± {{ totalAlarms }} Êù°\r\n      </div>\r\n      <div class=\"pagination-controls\">\r\n        <button @click=\"handlePageChange(currentPage - 1)\" :disabled=\"currentPage === 1\" class=\"pagination-btn\">\r\n          ‚Äπ\r\n        </button>\r\n        <input \r\n          type=\"number\" \r\n          :value=\"currentPage\"\r\n          @change=\"handlePageChange(Number($event.target.value))\"\r\n          class=\"page-input\"\r\n          min=\"1\"\r\n          :max=\"Math.ceil(totalAlarms / pageSize)\"\r\n        />\r\n        <span class=\"pagination-separator\">/</span>\r\n        <span class=\"total-pages\">{{ Math.ceil(totalAlarms / pageSize) || 1 }}</span>\r\n        <button @click=\"handlePageChange(currentPage + 1)\" :disabled=\"currentPage >= Math.ceil(totalAlarms / pageSize)\" class=\"pagination-btn\">\r\n          ‚Ä∫\r\n        </button>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- Áä∂ÊÄÅÊõ¥Êñ∞ÂØπËØùÊ°Ü -->\r\n    <div v-if=\"showStatusDialog\" class=\"modal-overlay\" @click.self=\"showStatusDialog = false\">\r\n      <div class=\"modal-premium\">\r\n        <div class=\"modal-header\">\r\n          <h3 class=\"modal-title\">Êõ¥Êñ∞ÂëäË≠¶Áä∂ÊÄÅ</h3>\r\n          <button @click=\"showStatusDialog = false\" class=\"modal-close\">√ó</button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <div class=\"info-row\">\r\n            <span class=\"info-label\">ÂëäË≠¶ID:</span>\r\n            <span class=\"info-value\">{{ currentAlarm?.id }}</span>\r\n          </div>\r\n          <div class=\"info-row\">\r\n            <span class=\"info-label\">ÂëäË≠¶ÊèèËø∞:</span>\r\n            <span class=\"info-value\">{{ currentAlarm?.content }}</span>\r\n          </div>\r\n          <div class=\"info-row\">\r\n            <span class=\"info-label\">ÂΩìÂâçÁä∂ÊÄÅ:</span>\r\n            <span class=\"status-badge\" :class=\"`status-${currentAlarm?.status.toLowerCase()}`\">\r\n              {{ getStatusText(currentAlarm?.status) }}\r\n            </span>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">Êõ¥Êñ∞‰∏∫</label>\r\n            <select v-model=\"newAlarmStatus\" class=\"form-select\">\r\n              <option value=\"PENDING\">ÂæÖÂ§ÑÁêÜ</option>\r\n              <option value=\"PROCESSING\">Â§ÑÁêÜ‰∏≠</option>\r\n              <option value=\"COMPLETED\">Â∑≤ÂÆåÊàê</option>\r\n              <option value=\"IGNORED\">Â∑≤ÂøΩÁï•</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button @click=\"showStatusDialog = false\" class=\"modal-btn secondary-btn\">ÂèñÊ∂à</button>\r\n          <button @click=\"confirmStatusUpdate\" class=\"modal-btn primary-btn\">Á°ÆÂÆö</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ËØ¶ÊÉÖÂØπËØùÊ°Ü -->\r\n    <div v-if=\"showDetailDialog\" class=\"modal-overlay\" @click.self=\"showDetailDialog = false\">\r\n      <div class=\"modal-premium detail-modal\">\r\n        <div class=\"modal-header\">\r\n          <h3 class=\"modal-title\">ÂëäË≠¶ËØ¶ÊÉÖ</h3>\r\n          <button @click=\"showDetailDialog = false\" class=\"modal-close\">√ó</button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <div class=\"detail-grid\">\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">ÂëäË≠¶ID</span>\r\n              <span class=\"detail-value\">{{ currentAlarm?.id }}</span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">ÂëäË≠¶Á±ªÂûã</span>\r\n              <span class=\"category-badge\" :class=\"getCategoryClass(currentAlarm?.category_name)\">\r\n                {{ currentAlarm?.category_name || 'Êú™ÂàÜÁ±ª' }}\r\n              </span>\r\n            </div>\r\n            <div class=\"detail-item full-width\">\r\n              <span class=\"detail-label\">ÂëäË≠¶ÊèèËø∞</span>\r\n              <span class=\"detail-value\">{{ currentAlarm?.content }}</span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">Êä•Ë≠¶‰ΩçÁΩÆ</span>\r\n              <span class=\"detail-value\">ÂùêÊ†á({{ currentAlarm?.latitude }}, {{ currentAlarm?.longitude }})</span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">Êä•Ë≠¶Êó∂Èó¥</span>\r\n              <span class=\"detail-value\">{{ formatDate(currentAlarm?.created_at) }}</span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">ÂëäË≠¶Áä∂ÊÄÅ</span>\r\n              <span class=\"status-badge\" :class=\"`status-${currentAlarm?.status.toLowerCase()}`\">\r\n                {{ getStatusText(currentAlarm?.status) }}\r\n              </span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">Ëà™Á∫ø‰ø°ÊÅØ</span>\r\n              <span class=\"detail-value\">{{ getWaylineName(currentAlarm) }} ({{ getWaylineId(currentAlarm) }})</span>\r\n            </div>\r\n            <div v-if=\"currentAlarm?.image_url\" class=\"detail-item full-width\">\r\n              <span class=\"detail-label\">Êä•Ë≠¶ÂõæÁâá</span>\r\n              <div class=\"alarm-image\">\r\n                <img :src=\"currentAlarm.image_url\" alt=\"ÂëäË≠¶ÂõæÁâá\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button @click=\"showDetailDialog = false\" class=\"modal-btn secondary-btn\">ÂÖ≥Èó≠</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport alarmApi from '../api/alarmApi.js'\r\nimport waylineApi from '../api/waylineApi.js'\r\n\r\nexport default {\r\n  name: 'AlarmList',\r\n  data() {\r\n    return {\r\n      alarms: [],\r\n      waylines: [],\r\n      filteredAlarms: [],\r\n      searchQuery: '',\r\n      statusFilter: '',\r\n      waylineIdFilter: '',\r\n      loading: false,\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      totalAlarms: 0,\r\n      showStatusDialog: false,\r\n      showDetailDialog: false,\r\n      currentAlarm: null,\r\n      newAlarmStatus: ''\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadAlarms()\r\n    this.loadWaylines()\r\n  },\r\n  methods: {\r\n    async loadAlarms() {\r\n      this.loading = true\r\n      try {\r\n        const params = {\r\n          page: this.currentPage,\r\n          page_size: this.pageSize\r\n        }\r\n        if (this.searchQuery) params.search = this.searchQuery\r\n        if (this.statusFilter) params.status = this.statusFilter\r\n        if (this.waylineIdFilter) params.wayline_id = this.waylineIdFilter\r\n        \r\n        const response = await alarmApi.getAlarms(params)\r\n        this.alarms = response.results || response\r\n        this.totalAlarms = response.count || this.alarms.length\r\n        this.filteredAlarms = this.alarms\r\n      } catch (error) {\r\n        console.error('Âä†ËΩΩÂëäË≠¶Â§±Ë¥•:', error)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    async loadWaylines() {\r\n      try {\r\n        const response = await waylineApi.getWaylines()\r\n        this.waylines = response.results || response\r\n      } catch (error) {\r\n        console.error('Âä†ËΩΩËà™Á∫øÂ§±Ë¥•:', error)\r\n      }\r\n    },\r\n    handleSearch() {\r\n      clearTimeout(this.searchTimeout)\r\n      this.searchTimeout = setTimeout(() => {\r\n        this.currentPage = 1\r\n        this.loadAlarms()\r\n      }, 300)\r\n    },\r\n    handlePageChange(page) {\r\n      if (page < 1 || page > Math.ceil(this.totalAlarms / this.pageSize)) return\r\n      this.currentPage = page\r\n      this.loadAlarms()\r\n    },\r\n    formatDate(dateString) {\r\n      if (!dateString) return '--'\r\n      const date = new Date(dateString)\r\n      return date.toLocaleString('zh-CN', {\r\n        year: 'numeric',\r\n        month: '2-digit',\r\n        day: '2-digit',\r\n        hour: '2-digit',\r\n        minute: '2-digit'\r\n      })\r\n    },\r\n    getWaylineId(alarm) {\r\n      return alarm.wayline_details?.wayline_id || alarm.wayline_id || '--'\r\n    },\r\n    getWaylineName(alarm) {\r\n      return alarm.wayline_details?.name || alarm.wayline?.name || 'Êú™Áü•Ëà™Á∫ø'\r\n    },\r\n    getStatusText(status) {\r\n      const statusMap = {\r\n        'PENDING': 'ÂæÖÂ§ÑÁêÜ',\r\n        'PROCESSING': 'Â§ÑÁêÜ‰∏≠',\r\n        'COMPLETED': 'Â∑≤ÂÆåÊàê',\r\n        'IGNORED': 'Â∑≤ÂøΩÁï•'\r\n      }\r\n      return statusMap[status] || status\r\n    },\r\n    getCategoryClass(category) {\r\n      if (!category) return ''\r\n      return `category-${category.toLowerCase().replace(/\\s+/g, '-')}`\r\n    },\r\n    viewAlarmDetail(alarm) {\r\n      this.currentAlarm = alarm\r\n      this.showDetailDialog = true\r\n    },\r\n    updateAlarmStatus(alarm) {\r\n      this.currentAlarm = alarm\r\n      this.newAlarmStatus = alarm.status\r\n      this.showStatusDialog = true\r\n    },\r\n    async confirmStatusUpdate() {\r\n      try {\r\n        await alarmApi.patchAlarm(this.currentAlarm.id, { status: this.newAlarmStatus })\r\n        this.showStatusDialog = false\r\n        this.loadAlarms()\r\n      } catch (error) {\r\n        console.error('Êõ¥Êñ∞Áä∂ÊÄÅÂ§±Ë¥•:', error)\r\n      }\r\n    },\r\n    async deleteAlarm(id) {\r\n      if (!confirm('Á°ÆÂÆöË¶ÅÂà†Èô§ËøôÊù°ÂëäË≠¶ÂêóÔºü')) return\r\n      try {\r\n        await alarmApi.deleteAlarm(id)\r\n        this.loadAlarms()\r\n      } catch (error) {\r\n        console.error('Âà†Èô§ÂëäË≠¶Â§±Ë¥•:', error)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.alarm-list-premium {\r\n  background: rgba(26, 31, 58, 0.6);\r\n  backdrop-filter: blur(10px);\r\n  border-radius: 16px;\r\n  border: 1px solid rgba(239, 68, 68, 0.2);\r\n  padding: 24px;\r\n}\r\n\r\n.list-header {\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.list-title {\r\n  font-size: 20px;\r\n  font-weight: 700;\r\n  background: linear-gradient(135deg, #ef4444 0%, #dc2626 100%);\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  background-clip: text;\r\n  margin: 0;\r\n}\r\n\r\n/* ÊêúÁ¥¢Á≠õÈÄâ */\r\n.search-filters-premium {\r\n  display: flex;\r\n  gap: 16px;\r\n  margin-bottom: 24px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.search-wrapper {\r\n  flex: 1;\r\n  min-width: 250px;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 12px;\r\n  padding: 12px 16px;\r\n  background: rgba(10, 14, 39, 0.6);\r\n  border-radius: 10px;\r\n  border: 1px solid rgba(239, 68, 68, 0.2);\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.search-wrapper:focus-within {\r\n  border-color: #ef4444;\r\n  box-shadow: 0 0 20px rgba(239, 68, 68, 0.2);\r\n}\r\n\r\n.search-icon {\r\n  font-size: 16px;\r\n  opacity: 0.7;\r\n}\r\n\r\n.search-input {\r\n  flex: 1;\r\n  background: none;\r\n  border: none;\r\n  outline: none;\r\n  c