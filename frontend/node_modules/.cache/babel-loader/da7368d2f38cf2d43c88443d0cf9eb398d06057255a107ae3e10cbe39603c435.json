{"ast":null,"code":"export default {\n  name: 'ControlPanel',\n  props: {\n    currentStatus: {\n      type: String,\n      default: '待命'\n    },\n    targetAltitude: {\n      type: Number,\n      default: 0\n    },\n    targetSpeed: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      localTargetAltitude: this.targetAltitude,\n      localTargetSpeed: this.targetSpeed,\n      isProcessingEmergencyStop: false\n    };\n  },\n  watch: {\n    targetAltitude(newVal) {\n      this.localTargetAltitude = newVal;\n    },\n    targetSpeed(newVal) {\n      this.localTargetSpeed = newVal;\n    }\n  },\n  methods: {\n    getStatusType(status) {\n      const statusMap = {\n        '待命': '',\n        '飞行中': 'success',\n        '悬停中': 'warning',\n        '返回中': 'info',\n        '已停止': 'danger'\n      };\n      return statusMap[status] || '';\n    },\n    // 处理控制动作\n    handleControl(action) {\n      this.$emit('control-action', action);\n    },\n    // 处理高度变更\n    handleAltitudeChange() {\n      this.$emit('update-altitude', this.localTargetAltitude);\n    },\n    // 处理速度变更\n    handleSpeedChange() {\n      this.$emit('update-speed', this.localTargetSpeed);\n    },\n    // 处理紧急停止\n    handleEmergencyStop() {\n      if (this.isProcessingEmergencyStop) return;\n      this.isProcessingEmergencyStop = true;\n      this.$emit('emergency-stop');\n\n      // 3秒后重置状态\n      setTimeout(() => {\n        this.isProcessingEmergencyStop = false;\n      }, 3000);\n    }\n  }\n};","map":{"version":3,"names":["name","props","currentStatus","type","String","default","targetAltitude","Number","targetSpeed","data","localTargetAltitude","localTargetSpeed","isProcessingEmergencyStop","watch","newVal","methods","getStatusType","status","statusMap","handleControl","action","$emit","handleAltitudeChange","handleSpeedChange","handleEmergencyStop","setTimeout"],"sources":["D:\\projects\\FlightHub-2-Frontend-Standalone-Component\\frontend\\src\\components\\ControlPanel.vue"],"sourcesContent":["<template>\n  <div class=\"control-panel\">\n    <div class=\"control-header\">\n      <h3 class=\"control-title\">无人机控制</h3>\n      <el-tag :type=\"getStatusType(currentStatus)\">\n        {{ currentStatus }}\n      </el-tag>\n    </div>\n    \n    <div class=\"control-actions\">\n      <div class=\"main-controls\">\n        <el-row :gutter=\"10\">\n          <el-col :span=\"12\">\n            <el-button \n              type=\"primary\" \n              size=\"large\"\n              class=\"control-button\"\n              @click=\"handleControl('start')\"\n              :disabled=\"currentStatus === '飞行中'\"\n            >\n              <i class=\"el-icon-video-play\"></i>\n              <span>启动</span>\n            </el-button>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-button \n              type=\"warning\" \n              size=\"large\"\n              class=\"control-button\"\n              @click=\"handleControl('pause')\"\n              :disabled=\"currentStatus !== '飞行中'\"\n            >\n              <i class=\"el-icon-video-pause\"></i>\n              <span>暂停</span>\n            </el-button>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-button \n              type=\"danger\" \n              size=\"large\"\n              class=\"control-button\"\n              @click=\"handleControl('stop')\"\n              :disabled=\"currentStatus === '待命' || currentStatus === '已停止'\"\n            >\n              <i class=\"el-icon-video-camera\"></i>\n              <span>停止</span>\n            </el-button>\n          </el-col>\n          <el-col :span=\"12\">\n            <el-button \n              type=\"info\" \n              size=\"large\"\n              class=\"control-button\"\n              @click=\"handleControl('return')\"\n              :disabled=\"currentStatus === '待命' || currentStatus === '已停止' || currentStatus === '返回中'\"\n            >\n              <i class=\"el-icon-refresh-left\"></i>\n              <span>返航</span>\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n      \n      <div class=\"control-sliders\">\n        <div class=\"slider-group\">\n          <div class=\"slider-header\">\n            <label class=\"slider-label\">目标高度</label>\n            <el-tag type=\"primary\" size=\"small\">{{ targetAltitude }} m</el-tag>\n          </div>\n          <el-slider\n            v-model.number=\"localTargetAltitude\"\n            :min=\"0\"\n            :max=\"100\"\n            :step=\"1\"\n            @change=\"handleAltitudeChange\"\n            :disabled=\"currentStatus !== '飞行中'\"\n            show-stops\n          />\n        </div>\n        \n        <div class=\"slider-group\">\n          <div class=\"slider-header\">\n            <label class=\"slider-label\">目标速度</label>\n            <el-tag type=\"primary\" size=\"small\">{{ targetSpeed }} m/s</el-tag>\n          </div>\n          <el-slider\n            v-model.number=\"localTargetSpeed\"\n            :min=\"0\"\n            :max=\"15\"\n            :step=\"0.5\"\n            @change=\"handleSpeedChange\"\n            :disabled=\"currentStatus !== '飞行中'\"\n            show-stops\n          />\n        </div>\n      </div>\n      \n      <div class=\"emergency-stop\">\n        <el-button \n          type=\"danger\" \n          size=\"large\"\n          class=\"emergency-button\"\n          @click=\"handleEmergencyStop\"\n          :loading=\"isProcessingEmergencyStop\"\n        >\n          <i class=\"el-icon-warning-filled\"></i>\n          <span>紧急停止</span>\n        </el-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ControlPanel',\n  props: {\n    currentStatus: {\n      type: String,\n      default: '待命'\n    },\n    targetAltitude: {\n      type: Number,\n      default: 0\n    },\n    targetSpeed: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      localTargetAltitude: this.targetAltitude,\n      localTargetSpeed: this.targetSpeed,\n      isProcessingEmergencyStop: false\n    }\n  },\n  watch: {\n    targetAltitude(newVal) {\n      this.localTargetAltitude = newVal\n    },\n    targetSpeed(newVal) {\n      this.localTargetSpeed = newVal\n    }\n  },\n  methods: {\n    getStatusType(status) {\n      const statusMap = {\n        '待命': '',\n        '飞行中': 'success',\n        '悬停中': 'warning',\n        '返回中': 'info',\n        '已停止': 'danger'\n      }\n      return statusMap[status] || ''\n    },\n    \n    // 处理控制动作\n    handleControl(action) {\n      this.$emit('control-action', action)\n    },\n    \n    // 处理高度变更\n    handleAltitudeChange() {\n      this.$emit('update-altitude', this.localTargetAltitude)\n    },\n    \n    // 处理速度变更\n    handleSpeedChange() {\n      this.$emit('update-speed', this.localTargetSpeed)\n    },\n    \n    // 处理紧急停止\n    handleEmergencyStop() {\n      if (this.isProcessingEmergencyStop) return\n      \n      this.isProcessingEmergencyStop = true\n      this.$emit('emergency-stop')\n      \n      // 3秒后重置状态\n      setTimeout(() => {\n        this.isProcessingEmergencyStop = false\n      }, 3000)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.control-panel {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n}\n\n.control-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10px 15px;\n  border-bottom: 1px solid #ebeef5;\n  background-color: #f5f7fa;\n}\n\n.control-title {\n  margin: 0;\n  font-size: 16px;\n  font-weight: bold;\n  color: #303133;\n}\n\n.control-actions {\n  flex: 1;\n  padding: 15px;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.main-controls {\n  flex: 1;\n}\n\n.control-button {\n  width: 100%;\n  height: 60px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  font-size: 14px;\n}\n\n.control-button i {\n  font-size: 20px;\n  margin-bottom: 5px;\n}\n\n.control-sliders {\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.slider-group {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.slider-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.slider-label {\n  font-size: 14px;\n  color: #606266;\n}\n\n.emergency-button {\n  width: 100%;\n  height: 50px;\n  font-size: 16px;\n  font-weight: bold;\n}\n\n.emergency-button i {\n  margin-right: 8px;\n}\n</style>"],"mappings":"AAkHA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE;IACLC,aAAa,EAAE;MACbC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,cAAc,EAAE;MACdH,IAAI,EAAEI,MAAM;MACZF,OAAO,EAAE;IACX,CAAC;IACDG,WAAW,EAAE;MACXL,IAAI,EAAEI,MAAM;MACZF,OAAO,EAAE;IACX;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,mBAAmB,EAAE,IAAI,CAACJ,cAAc;MACxCK,gBAAgB,EAAE,IAAI,CAACH,WAAW;MAClCI,yBAAyB,EAAE;IAC7B;EACF,CAAC;EACDC,KAAK,EAAE;IACLP,cAAcA,CAACQ,MAAM,EAAE;MACrB,IAAI,CAACJ,mBAAkB,GAAII,MAAK;IAClC,CAAC;IACDN,WAAWA,CAACM,MAAM,EAAE;MAClB,IAAI,CAACH,gBAAe,GAAIG,MAAK;IAC/B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,MAAM,EAAE;MACpB,MAAMC,SAAQ,GAAI;QAChB,IAAI,EAAE,EAAE;QACR,KAAK,EAAE,SAAS;QAChB,KAAK,EAAE,SAAS;QAChB,KAAK,EAAE,MAAM;QACb,KAAK,EAAE;MACT;MACA,OAAOA,SAAS,CAACD,MAAM,KAAK,EAAC;IAC/B,CAAC;IAED;IACAE,aAAaA,CAACC,MAAM,EAAE;MACpB,IAAI,CAACC,KAAK,CAAC,gBAAgB,EAAED,MAAM;IACrC,CAAC;IAED;IACAE,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAACD,KAAK,CAAC,iBAAiB,EAAE,IAAI,CAACX,mBAAmB;IACxD,CAAC;IAED;IACAa,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACF,KAAK,CAAC,cAAc,EAAE,IAAI,CAACV,gBAAgB;IAClD,CAAC;IAED;IACAa,mBAAmBA,CAAA,EAAG;MACpB,IAAI,IAAI,CAACZ,yBAAyB,EAAE;MAEpC,IAAI,CAACA,yBAAwB,GAAI,IAAG;MACpC,IAAI,CAACS,KAAK,CAAC,gBAAgB;;MAE3B;MACAI,UAAU,CAAC,MAAM;QACf,IAAI,CAACb,yBAAwB,GAAI,KAAI;MACvC,CAAC,EAAE,IAAI;IACT;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}