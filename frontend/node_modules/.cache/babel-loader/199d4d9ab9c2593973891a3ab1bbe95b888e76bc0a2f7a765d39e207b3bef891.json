v-if=\"currentAlarm\" class=\"alarm-detail-premium\">\n        <div class=\"detail-grid\">\n          <div class=\"detail-item\">\n            <label>ÂëäË≠¶ID</label>\n            <div class=\"value\">{{ currentAlarm.id }}</div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Á±ªÂûã</label>\n            <div class=\"value\">{{ currentAlarm.category_details?.name || 'Êú™ÂàÜÁ±ª' }}</div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Áä∂ÊÄÅ</label>\n            <div class=\"value\">\n              <span :class=\"['status-tag', `status-${currentAlarm.status.toLowerCase()}`]\">\n                {{ getStatusText(currentAlarm.status) }}\n              </span>\n            </div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Êó∂Èó¥</label>\n            <div class=\"value\">{{ currentAlarm.created_at ? new Date(currentAlarm.created_at).toLocaleString('zh-CN') : '--' }}</div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>‰ΩçÁΩÆ</label>\n            <div class=\"value font-mono\">\n              <span class=\"coord\">LAT: {{ currentAlarm.latitude || '--' }}</span>\n              <span class=\"coord\">LNG: {{ currentAlarm.longitude || '--' }}</span>\n            </div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>ÂÖ≥ËÅîËà™Á∫ø</label>\n            <div class=\"value\">{{ currentAlarm.wayline?.name || currentAlarm.wayline_details?.name || 'Êú™Áü•Ëà™Á∫ø' }}</div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>ÊèèËø∞</label>\n            <div class=\"value description-box\">{{ currentAlarm.content || '--' }}</div>\n          </div>\n          \n          <div v-if=\"currentAlarm.image_url\" class=\"detail-item full-width\">\n            <label>Áé∞Âú∫ÂõæÁâá</label>\n            <div class=\"image-box\">\n              <el-image\n                :src=\"currentAlarm.image_url\"\n                :preview-src-list=\"[currentAlarm.image_url]\"\n                fit=\"contain\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n      \n      <template #footer>\n        <div class=\"dialog-footer\">\n          <button class=\"premium-btn\" @click=\"showAlarmDetail = false\">ÂÖ≥Èó≠</button>\n        </div>\n      </template>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { DjiWaylineCreation } from '../djispace-components'\nimport TaskProgressBar from '../components/TaskProgressBar.vue'\nimport AlarmPanel from '../components/AlarmPanel.vue'\nimport WaylineFallback from '../components/WaylineFallback.vue'\nimport alarmApi from '../api/alarmApi.js'\n\nexport default {\n  name: 'DjiDashboard',\n  components: {\n    DjiWaylineCreation,\n    TaskProgressBar,\n    AlarmPanel,\n    WaylineFallback\n  },\n  data() {\n    return {\n      showWaylineCreation: false,\n      taskProgress: 65,\n      currentTask: 'ÂèòÁîµÁ´ôËÆæÂ§áÊ£ÄÊü•',\n      remainingTime: '12:45',\n      completedTasks: 8,\n      totalTasks: 12,\n      // CesiumÁõ∏ÂÖ≥Êï∞ÊçÆ\n      viewer: null,\n      tileset: null,\n      loading: false,\n      error: '',\n  \n      resizeObserver: null,\n      debouncedForceResize: null,\n      // Â§ßÁñÜÁªÑ‰ª∂Âä†ËΩΩÁä∂ÊÄÅ\n      fh2Loaded: false,\n      // ÂÖ®Â±ÄÂèòÈáèÔºöÂΩìÂâçÈÄâ‰∏≠ÁöÑËà™Á∫ø\n      selectedWayline: null,\n      // ÂëäË≠¶‰ø°ÊÅØ\n      alarms: [],\n      // ÂëäË≠¶Âä†ËΩΩÁä∂ÊÄÅ\n      loadingAlarms: false,\n      // ÂëäË≠¶ËØ¶ÊÉÖÂºπÁ™ó\n      showAlarmDetail: false,\n      currentAlarm: null,\n      // ÈÖçÁΩÆ‰ø°ÊÅØ\n      djiConfig: {\n        serverUrl: 'http://your-dji-server-ip-or-domain:30812',\n        wssUrl: 'ws://your-dji-server-ip-or-domain:30812/duplex/web',\n        hostUrl: 'http://your-dji-server-ip-or-domain',\n        prjId: 'cdca2736-8096-4729-9bc7-497ad5b60d5e',\n        projectToken: 'your-project-token-here'\n      }\n    }\n  },\n  async mounted() {\n    this.checkFh2Availability()\n    \n    this.$nextTick(async () => {\n      setTimeout(async () => {\n        await this.initCesium()\n        this.initResizeObserver()\n        await this.loadModel()\n      }, 200)\n    })\n  },\n  beforeUnmount() {\n    if (this.viewer) {\n      this.viewer.destroy()\n    }\n    if (this.resizeObserver) {\n      this.resizeObserver.disconnect()\n    }\n    window.removeEventListener('resize', this.handleWindowResize)\n    if (this.debouncedForceResize && this.debouncedForceResize.timeout) {\n      clearTimeout(this.debouncedForceResize.timeout)\n    }\n  },\n  methods: {\n    checkFh2Availability() {\n      if (typeof window.FH2 !== 'undefined') {\n        this.fh2Loaded = true\n      } else {\n        this.fh2Loaded = false\n        setTimeout(() => {\n          this.checkFh2Availability()\n        }, 1000)\n      }\n    },\n    \n    openWaylineCreation() {\n      if (!this.fh2Loaded) {\n        this.$message.warning('Â§ßÁñÜÁªÑ‰ª∂Êú™Ê≠£Á°ÆÂä†ËΩΩÔºåÊó†Ê≥ïÂàõÂª∫Ëà™Á∫ø')\n        return\n      }\n      this.showWaylineCreation = true\n    },\n\n    handleAlarmRefresh() {\n      if (this.selectedWayline) {\n        this.fetchAlarmsByWayline(this.selectedWayline.id)\n      }\n    },\n\n    handleViewAlarmDetail(alarm) {\n      this.currentAlarm = alarm\n      this.showAlarmDetail = true\n    },\n\n    handleProcessAlarm(alarmId) {\n      this.alarms = this.alarms.filter(alarm => alarm.id !== alarmId)\n      this.updateAlarmStatus(alarmId)\n    },\n    \n    async updateAlarmStatus(alarmId) {\n      try {\n        await alarmApi.patchAlarm(alarmId, { status: 'COMPLETED' })\n      } catch (error) {\n        console.error('Êõ¥Êñ∞ÂëäË≠¶Áä∂ÊÄÅÂ§±Ë¥•:', error)\n      }\n    },\n    \n    handleWaylineSelected(wayline) {\n      this.selectedWayline = wayline\n      this.fetchAlarmsByWayline(wayline.id)\n    },\n    \n    async fetchAlarmsByWayline(waylineId) {\n      if (!waylineId) {\n        this.alarms = []\n        return\n      }\n      \n      this.loadingAlarms = true\n      try {\n        const response = await alarmApi.getAlarms({ wayline_id: waylineId })\n        this.alarms = this.transformAlarmData(response)\n      } catch (error) {\n        console.error('Ëé∑ÂèñÂëäË≠¶‰ø°ÊÅØÂ§±Ë¥•:', error)\n        this.alarms = []\n      } finally {\n        this.loadingAlarms = false\n      }\n    },\n    \n    transformAlarmData(alarmData) {\n      return Array.isArray(alarmData) ? alarmData : (alarmData.results || [])\n    },\n    \n    getFilteredAlarms() {\n      return this.alarms\n    },\n    \n    getStatusText(status) {\n      const statusMap = {\n        'PENDING': 'ÂæÖÂ§ÑÁêÜ',\n        'PROCESSING': 'Â§ÑÁêÜ‰∏≠',\n        'COMPLETED': 'Â∑≤ÂÆåÊàê',\n        'IGNORED': 'Â∑≤ÂøΩÁï•'\n      }\n      return statusMap[status] || status\n    },\n\n    async initCesium() {\n      try {\n        const Cesium = await import('cesium')\n        Cesium.Ion.defaultAccessToken = ''\n        \n        const wrapper = this.$refs.cesiumWrapper\n        if (!wrapper) return\n        \n        const existingContainer = document.getElementById('cesiumContainer')\n        if (existingContainer) existingContainer.remove()\n        \n        const container = document.createElement('div')\n        container.id = 'cesiumContainer'\n        container.style.width = '100%'\n        container.style.height = '100%'\n        container.style.position = 'absolute'\n        container.style.top = '0'\n        container.style.left = '0'\n        container.style.overflow = 'hidden'\n        \n        wrapper.appendChild(container)\n        \n        this.viewer = new Cesium.Viewer(container, {\n          animation: false,\n          baseLayerPicker: false,\n          fullscreenButton: false,\n          vrButton: false,\n          geocoder: false,\n          homeButton: false,\n          infoBox: false,\n          sceneModePicker: false,\n          selectionIndicator: false,\n          timeline: false,\n          navigationHelpButton: false,\n          navigationInstructionsInitiallyVisible: false,\n          scene3DOnly: true,\n          shadows: true,\n          shouldAnimate: true,\n          creditContainer: document.createElement('div'),\n          automaticallyTrackDataSourceClocks: false,\n          orderIndependentTranslucency: true,\n          contextOptions: {\n            alpha: true,\n            stencil: true\n          }\n        })\n        \n        this.viewer.camera.setView({\n          destination: Cesium.Cartesian3.fromDegrees(116.3913, 39.9075, 1000),\n          orientation: {\n            heading: Cesium.Math.toRadians(0),\n            pitch: Cesium.Math.toRadians(-30),\n            roll: 0.0\n          }\n        })\n      } catch (err) {\n        this.error = 'ÂàùÂßãÂåñCesiumÂ§±Ë¥•: ' + err.message\n      }\n    },\n\n    debounce(func, wait) {\n      let timeout;\n      return function(...args) {\n        clearTimeout(timeout);\n        timeout = setTimeout(() => func(...args), wait);\n      };\n    },\n    \n    initResizeObserver() {\n      const wrapper = this.$refs.cesiumWrapper\n      if (!wrapper) return\n      \n      if (this.resizeObserver) this.resizeObserver.disconnect()\n      \n      this.debouncedForceResize = this.debounce(() => {\n        if (this.viewer) this.forceResize()\n      }, 100)\n      \n      this.resizeObserver = new ResizeObserver(() => {\n        this.debouncedForceResize()\n      })\n      \n      this.resizeObserver.observe(wrapper, { box: 'content-box' })\n      window.addEventListener('resize', this.handleWindowResize)\n      setTimeout(() => this.forceResize(), 100)\n    },\n    \n    handleWindowResize() {\n      if (this.viewer) this.debouncedForceResize()\n    },\n\n    forceResize() {\n      try {\n        const wrapper = this.$refs.cesiumWrapper\n        const container = document.getElementById('cesiumContainer')\n        \n        if (!wrapper || !container || !this.viewer) return\n        \n        // Âº∫Âà∂ÈáçÊñ∞ËÆ°ÁÆó\n        if (this.viewer.scene) this.viewer.scene.resize()\n        if (this.viewer.resize) this.viewer.resize()\n      } catch (error) {\n        console.error('Ë∞ÉÊï¥Â§ßÂ∞èÂ§±Ë¥•:', error)\n      }\n    },\n\n    async loadModel() {\n      if (!this.viewer) return\n      \n      this.loading = true\n      this.error = ''\n      \n      try {\n        const Cesium = await import('cesium')\n        if (this.tileset) {\n          this.viewer.scene.primitives.remove(this.tileset)\n          this.tileset = null\n        }\n        \n        this.tileset = await Cesium.Cesium3DTileset.fromUrl('/models/Model_0/tileset.json')\n        this.viewer.scene.primitives.add(this.tileset)\n        \n        if (this.tileset.readyPromise) {\n          this.tileset.readyPromise.then(() => this.flyToModel())\n            .catch(err => this.error = 'Ê®°ÂûãÂä†ËΩΩÂ§±Ë¥•: ' + err.message)\n            .finally(() => this.loading = false)\n        } else {\n          this.flyToModel()\n          this.loading = false\n        }\n      } catch (err) {\n        this.error = 'Ê®°ÂûãÂä†ËΩΩÂ§±Ë¥•: ' + err.message\n        this.loading = false\n      }\n    },\n\n    flyToModel() {\n      if (this.viewer && this.tileset) {\n        this.viewer.flyTo(this.tileset)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* Âü∫Á°ÄÂ∏ÉÂ±Ä */\n.dji-dashboard-premium {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  overflow: hidden;\n  background-color: #0a0e27;\n}\n\n.dashboard-content {\n  display: flex;\n  flex: 1;\n  overflow: hidden;\n  padding: 16px;\n  gap: 16px;\n}\n\n/* ‰æßËæπÈù¢Êùø */\n.side-panel {\n  width: 320px;\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n  flex-shrink: 0;\n}\n\n.center-view {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n  min-width: 0;\n}\n\n/* Âç°ÁâáÊ†∑Âºè */\n.panel-card {\n  background: rgba(26, 31, 58, 0.6);\n  backdrop-filter: blur(10px);\n  border-radius: 12px;\n  border: 1px solid rgba(0, 212, 255, 0.2);\n  display: flex;\n  flex-direction: column;\n  overflow: hidden;\n  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.2);\n}\n\n.left-panel .panel-card {\n  flex: 1;\n}\n\n.right-panel .panel-card {\n  flex: 1;\n  min-height: 200px;\n}\n\n.panel-header {\n  padding: 12px 16px;\n  background: linear-gradient(90deg, rgba(0, 212, 255, 0.1) 0%, transparent 100%);\n  border-bottom: 1px solid rgba(0, 212, 255, 0.1);\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.small-header {\n  padding: 8px 16px;\n}\n\n.panel-icon {\n  font-size: 18px;\n}\n\n.panel-title {\n  font-size: 15px;\n  font-weight: 600;\n  color: #00d4ff;\n  margin: 0;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.panel-sub-header {\n  padding: 8px 16px;\n  border-bottom: 1px solid rgba(0, 212, 255, 0.1);\n  background: rgba(0, 0, 0, 0.1);\n}\n\n.wayline-tag {\n  font-size: 12px;\n  color: #e0f2fe;\n  padding: 2px 8px;\n  background: rgba(0, 212, 255, 0.1);\n  border-radius: 4px;\n  border: 1px solid rgba(0, 212, 255, 0.2);\n}\n\n.wayline-tag.inactive {\n  color: #94a3b8;\n  background: rgba(148, 163, 184, 0.1);\n  border-color: rgba(148, 163, 184, 0.2);\n}\n\n.panel-body {\n  flex: 1;\n  padding: 16px;\n  overflow-y: auto;\n  position: relative;\n}\n\n.panel-body.no-padding {\n  padding: 0;\n}\n\n/* ËøõÂ∫¶Êù°Âç°Áâá */\n.progress-card {\n  background: rgba(26, 31, 58, 0.6);\n  backdrop-filter: blur(10px);\n  border-radius: 12px;\n  border: 1px solid rgba(0, 212, 255, 0.2);\n  flex-shrink: 0;\n}\n\n/* CesiumÂÆπÂô® */\n.cesium-container-wrapper {\n  flex: 1;\n  position: relative;\n  border-radius: 12px;\n  overflow: hidden;\n  border: 1px solid rgba(0, 212, 255, 0.2);\n  background: #0f172a;\n  box-shadow: 0 0 20px rgba(0, 212, 255, 0.1);\n}\n\n/* Âä†ËΩΩÂíåÈîôËØØÁä∂ÊÄÅ */\n.loading-overlay, .error-overlay {\n  position: absolute;\n  inset: 0;\n  background: rgba(10, 14, 39, 0.8);\n  backdrop-filter: blur(4px);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-c