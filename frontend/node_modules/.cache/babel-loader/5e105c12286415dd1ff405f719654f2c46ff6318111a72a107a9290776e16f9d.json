{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport alarmApi from '../api/alarmApi';\nexport default {\n  name: 'AlarmList',\n  data() {\n    return {\n      alarms: [],\n      categories: [],\n      loading: false,\n      searchQuery: '',\n      statusFilter: '',\n      currentPage: 1,\n      pageSize: 10,\n      totalAlarms: 0,\n      showDialog: false,\n      isEdit: false,\n      currentAlarmId: null,\n      dialogTitle: '新增告警',\n      alarmForm: {\n        category: '',\n        latitude: '',\n        longitude: '',\n        content: '',\n        image_url: '',\n        status: 'PENDING'\n      },\n      alarmRules: {\n        category: [{\n          required: true,\n          message: '请选择告警类型',\n          trigger: 'change'\n        }],\n        latitude: [{\n          required: true,\n          message: '请输入纬度',\n          trigger: 'blur'\n        }, {\n          type: 'number',\n          min: -90,\n          max: 90,\n          message: '纬度范围为-90到90',\n          trigger: 'blur'\n        }],\n        longitude: [{\n          required: true,\n          message: '请输入经度',\n          trigger: 'blur'\n        }, {\n          type: 'number',\n          min: -180,\n          max: 180,\n          message: '经度范围为-180到180',\n          trigger: 'blur'\n        }],\n        content: [{\n          required: true,\n          message: '请输入告警描述',\n          trigger: 'blur'\n        }, {\n          min: 5,\n          max: 500,\n          message: '描述长度在 5 到 500 个字符',\n          trigger: 'blur'\n        }],\n        status: [{\n          required: true,\n          message: '请选择状态',\n          trigger: 'change'\n        }]\n      }\n    };\n  },\n  computed: {\n    filteredAlarms() {\n      let filtered = [...this.alarms];\n\n      // 根据搜索关键词过滤\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase();\n        filtered = filtered.filter(alarm => alarm.content.toLowerCase().includes(query) || alarm.category_name.toLowerCase().includes(query));\n      }\n\n      // 根据状态过滤\n      if (this.statusFilter) {\n        filtered = filtered.filter(alarm => alarm.status === this.statusFilter);\n      }\n      return filtered;\n    }\n  },\n  async mounted() {\n    await this.loadAlarms();\n    await this.loadCategories();\n  },\n  methods: {\n    async loadAlarms() {\n      this.loading = true;\n      try {\n        const response = await alarmApi.getAlarms({\n          page: this.currentPage,\n          page_size: this.pageSize\n        });\n\n        // 转换数据格式，确保前端显示正确\n        this.alarms = response.results.map(alarm => ({\n          id: alarm.id,\n          created_at: alarm.created_at,\n          category_name: alarm.category ? alarm.category.name : '未分类',\n          category_id: alarm.category ? alarm.category.id : null,\n          latitude: alarm.latitude,\n          longitude: alarm.longitude,\n          content: alarm.content,\n          image_url: alarm.image_url,\n          status: alarm.status\n        }));\n        this.totalAlarms = response.count;\n      } catch (error) {\n        this.$message.error('获取告警列表失败');\n        console.error('Load alarms error:', error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async loadCategories() {\n      try {\n        const categories = await alarmApi.getAlarmCategories();\n        this.categories = categories;\n      } catch (error) {\n        this.$message.error('获取告警类型失败');\n        console.error('Load categories error:', error);\n      }\n    },\n    formatDate(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      });\n    },\n    getStatusText(status) {\n      const statusMap = {\n        'PENDING': '待处理',\n        'PROCESSING': '处理中',\n        'COMPLETED': '已完成',\n        'IGNORED': '已忽略'\n      };\n      return statusMap[status] || status;\n    },\n    getStatusType(status) {\n      const typeMap = {\n        'PENDING': 'danger',\n        'PROCESSING': 'warning',\n        'COMPLETED': 'success',\n        'IGNORED': 'info'\n      };\n      return typeMap[status] || 'default';\n    },\n    getCategoryType(categoryName) {\n      // 根据不同的告警类型返回不同的标签类型\n      const typeKeywords = {\n        '发热': 'danger',\n        '损伤': 'warning',\n        '故障': 'danger',\n        '异常': 'warning',\n        '隐患': 'warning'\n      };\n      for (const [keyword, type] of Object.entries(typeKeywords)) {\n        if (categoryName.includes(keyword)) {\n          return type;\n        }\n      }\n      return 'info';\n    },\n    openAddAlarmDialog() {\n      this.isEdit = false;\n      this.dialogTitle = '新增告警';\n      this.resetForm();\n      this.showDialog = true;\n    },\n    resetForm() {\n      this.alarmForm = {\n        category: '',\n        latitude: '',\n        longitude: '',\n        content: '',\n        image_url: '',\n        status: 'PENDING'\n      };\n      if (this.$refs.alarmForm) {\n        this.$refs.alarmForm.resetFields();\n      }\n    },\n    async submitAlarm() {\n      this.$refs.alarmForm.validate(async valid => {\n        if (valid) {\n          try {\n            if (this.isEdit) {\n              // 更新告警\n              await alarmApi.updateAlarm(this.currentAlarmId, this.alarmForm);\n              this.$message.success('告警更新成功');\n            } else {\n              // 创建新告警\n              await alarmApi.createAlarm(this.alarmForm);\n              this.$message.success('告警创建成功');\n            }\n            this.cancelDialog();\n            await this.loadAlarms(); // 重新加载列表\n          } catch (error) {\n            this.$message.error(this.isEdit ? '更新失败' : '创建失败');\n            console.error('Submit alarm error:', error);\n          }\n        }\n      });\n    },\n    cancelDialog() {\n      this.showDialog = false;\n      this.resetForm();\n    },\n    viewAlarmDetail(alarm) {\n      // 查看详情，可以打开一个新的详情对话框\n      this.$message.info(`查看告警ID: ${alarm.id} 的详情`);\n      // 这里可以实现更详细的查看功能\n    },\n    editAlarm(alarm) {\n      this.isEdit = true;\n      this.dialogTitle = '编辑告警';\n      this.currentAlarmId = alarm.id;\n\n      // 填充表单数据\n      this.alarmForm = {\n        category: alarm.category_id,\n        latitude: alarm.latitude,\n        longitude: alarm.longitude,\n        content: alarm.content,\n        image_url: alarm.image_url,\n        status: alarm.status\n      };\n      this.showDialog = true;\n    },\n    async deleteAlarm(alarmId) {\n      this.$confirm('确定要删除这条告警吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        try {\n          await alarmApi.deleteAlarm(alarmId);\n          this.$message.success('删除成功');\n          await this.loadAlarms(); // 重新加载列表\n        } catch (error) {\n          this.$message.error('删除失败');\n          console.error('Delete alarm error:', error);\n        }\n      }).catch(() => {\n        // 取消删除\n      });\n    },\n    handleSizeChange(size) {\n      this.pageSize = size;\n      this.loadAlarms();\n    },\n    handleCurrentChange(current) {\n      this.currentPage = current;\n      this.loadAlarms();\n    }\n  }\n};","map":{"version":3,"names":["alarmApi","name","data","alarms","categories","loading","searchQuery","statusFilter","currentPage","pageSize","totalAlarms","showDialog","isEdit","currentAlarmId","dialogTitle","alarmForm","category","latitude","longitude","content","image_url","status","alarmRules","required","message","trigger","type","min","max","computed","filteredAlarms","filtered","query","toLowerCase","filter","alarm","includes","category_name","mounted","loadAlarms","loadCategories","methods","response","getAlarms","page","page_size","results","map","id","created_at","category_id","count","error","$message","console","getAlarmCategories","formatDate","dateString","date","Date","toLocaleString","year","month","day","hour","minute","second","getStatusText","statusMap","getStatusType","typeMap","getCategoryType","categoryName","typeKeywords","keyword","Object","entries","openAddAlarmDialog","resetForm","$refs","resetFields","submitAlarm","validate","valid","updateAlarm","success","createAlarm","cancelDialog","viewAlarmDetail","info","editAlarm","deleteAlarm","alarmId","$confirm","confirmButtonText","cancelButtonText","then","catch","handleSizeChange","size","handleCurrentChange","current"],"sources":["D:\\projects\\FlightHub-2-Frontend-Standalone-Component\\frontend\\src\\components\\AlarmList.vue"],"sourcesContent":["<template>\n  <div class=\"alarm-list-container\">\n    <div class=\"header-actions\">\n      <h2 class=\"title\">告警信息列表</h2>\n      <el-button type=\"primary\" @click=\"openAddAlarmDialog\">新增告警</el-button>\n    </div>\n    \n    <!-- 搜索和筛选 -->\n    <div class=\"search-filters mb-4\">\n      <el-input\n        v-model=\"searchQuery\"\n        placeholder=\"搜索告警内容\"\n        prefix-icon=\"el-icon-search\"\n        class=\"search-input\"\n      />\n      <el-select\n        v-model=\"statusFilter\"\n        placeholder=\"筛选状态\"\n        class=\"status-filter\"\n      >\n        <el-option label=\"全部\" value=\"\" />\n        <el-option label=\"待处理\" value=\"PENDING\" />\n        <el-option label=\"处理中\" value=\"PROCESSING\" />\n        <el-option label=\"已完成\" value=\"COMPLETED\" />\n        <el-option label=\"已忽略\" value=\"IGNORED\" />\n      </el-select>\n    </div>\n    \n    <!-- 告警列表 -->\n    <el-table\n      v-loading=\"loading\"\n      :data=\"filteredAlarms\"\n      style=\"width: 100%\"\n      border\n      empty-text=\"暂无告警数据\"\n    >\n      <el-table-column prop=\"id\" label=\"告警ID\" width=\"80\" />\n      \n      <el-table-column prop=\"created_at\" label=\"报警日期时间\" width=\"180\">\n        <template #default=\"{row}\">\n          {{ formatDate(row?.created_at || '') }}\n        </template>\n      </el-table-column>\n      \n      <el-table-column prop=\"category_name\" label=\"报警类型\">\n        <template #default=\"{row}\">\n          <el-tag :type=\"getCategoryType(row?.category_name || '')\">\n            {{ row?.category_name || '未分类' }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      \n      <el-table-column prop=\"content\" label=\"告警描述\" min-width=\"200\" />\n      \n      <el-table-column prop=\"image_url\" label=\"报警图片\" width=\"100\">\n        <template #default=\"{row}\">\n          <el-image\n            v-if=\"row?.image_url\"\n            :src=\"row.image_url\"\n            :preview-src-list=\"[row.image_url]\"\n            style=\"width: 40px; height: 40px; cursor: pointer\"\n            fit=\"cover\"\n          />\n          <span v-else class=\"text-gray-400\">无图片</span>\n        </template>\n      </el-table-column>\n      \n      <el-table-column prop=\"location\" label=\"报警位置\">\n        <template #default=\"{row}\">\n          坐标({{ row?.latitude || '--' }}, {{ row?.longitude || '--' }})\n        </template>\n      </el-table-column>\n      \n      <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\n        <template #default=\"{row}\">\n          <el-tag :type=\"getStatusType(row?.status || '')\">\n            {{ getStatusText(row?.status || '') }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      \n      <el-table-column label=\"操作\" width=\"180\" fixed=\"right\">\n        <template #default=\"{row}\">\n          <el-button size=\"small\" @click=\"viewAlarmDetail(row)\">查看</el-button>\n          <el-button size=\"small\" type=\"primary\" @click=\"editAlarm(row)\">编辑</el-button>\n          <el-button size=\"small\" type=\"danger\" @click=\"deleteAlarm(row?.id || 0)\">删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    \n    <!-- 分页 -->\n    <div class=\"pagination-container\">\n      <el-pagination\n        v-model=\"currentPage\"\n        :page-size=\"pageSize\"\n        :total=\"totalAlarms\"\n        layout=\"total, prev, pager, next, jumper\"\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n      />\n    </div>\n    \n    <!-- 添加/编辑告警对话框 -->\n    <el-dialog\n      :title=\"dialogTitle\"\n      :visible.sync=\"showDialog\"\n      width=\"60%\"\n    >\n      <el-form\n        ref=\"alarmForm\"\n        :model=\"alarmForm\"\n        :rules=\"alarmRules\"\n        label-width=\"120px\"\n      >\n        <el-form-item label=\"告警类型\" prop=\"category\">\n          <el-select\n            v-model=\"alarmForm.category\"\n            placeholder=\"请选择告警类型\"\n          >\n            <el-option\n              v-for=\"category in categories\"\n              :key=\"category.id\"\n              :label=\"category.name\"\n              :value=\"category.id\"\n            />\n          </el-select>\n        </el-form-item>\n        \n        <el-form-item label=\"纬度\" prop=\"latitude\">\n          <el-input v-model.number=\"alarmForm.latitude\" placeholder=\"请输入纬度\" />\n        </el-form-item>\n        \n        <el-form-item label=\"经度\" prop=\"longitude\">\n          <el-input v-model.number=\"alarmForm.longitude\" placeholder=\"请输入经度\" />\n        </el-form-item>\n        \n        <el-form-item label=\"告警描述\" prop=\"content\">\n          <el-input\n            v-model=\"alarmForm.content\"\n            type=\"textarea\"\n            placeholder=\"请输入告警描述\"\n            rows=\"3\"\n          />\n        </el-form-item>\n        \n        <el-form-item label=\"告警图片\">\n          <el-input v-model=\"alarmForm.image_url\" placeholder=\"请输入图片URL\" />\n        </el-form-item>\n        \n        <el-form-item label=\"状态\" prop=\"status\">\n          <el-select v-model=\"alarmForm.status\" placeholder=\"请选择状态\">\n            <el-option label=\"待处理\" value=\"PENDING\" />\n            <el-option label=\"处理中\" value=\"PROCESSING\" />\n            <el-option label=\"已完成\" value=\"COMPLETED\" />\n            <el-option label=\"已忽略\" value=\"IGNORED\" />\n          </el-select>\n        </el-form-item>\n      </el-form>\n      \n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"cancelDialog\">取消</el-button>\n        <el-button type=\"primary\" @click=\"submitAlarm\">确定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport alarmApi from '../api/alarmApi'\n\nexport default {\n  name: 'AlarmList',\n  data() {\n    return {\n      alarms: [],\n      categories: [],\n      loading: false,\n      searchQuery: '',\n      statusFilter: '',\n      currentPage: 1,\n      pageSize: 10,\n      totalAlarms: 0,\n      showDialog: false,\n      isEdit: false,\n      currentAlarmId: null,\n      dialogTitle: '新增告警',\n      alarmForm: {\n        category: '',\n        latitude: '',\n        longitude: '',\n        content: '',\n        image_url: '',\n        status: 'PENDING'\n      },\n      alarmRules: {\n        category: [\n          { required: true, message: '请选择告警类型', trigger: 'change' }\n        ],\n        latitude: [\n          { required: true, message: '请输入纬度', trigger: 'blur' },\n          { type: 'number', min: -90, max: 90, message: '纬度范围为-90到90', trigger: 'blur' }\n        ],\n        longitude: [\n          { required: true, message: '请输入经度', trigger: 'blur' },\n          { type: 'number', min: -180, max: 180, message: '经度范围为-180到180', trigger: 'blur' }\n        ],\n        content: [\n          { required: true, message: '请输入告警描述', trigger: 'blur' },\n          { min: 5, max: 500, message: '描述长度在 5 到 500 个字符', trigger: 'blur' }\n        ],\n        status: [\n          { required: true, message: '请选择状态', trigger: 'change' }\n        ]\n      }\n    }\n  },\n  computed: {\n    filteredAlarms() {\n      let filtered = [...this.alarms]\n      \n      // 根据搜索关键词过滤\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase()\n        filtered = filtered.filter(alarm => \n          alarm.content.toLowerCase().includes(query) ||\n          alarm.category_name.toLowerCase().includes(query)\n        )\n      }\n      \n      // 根据状态过滤\n      if (this.statusFilter) {\n        filtered = filtered.filter(alarm => alarm.status === this.statusFilter)\n      }\n      \n      return filtered\n    }\n  },\n  async mounted() {\n    await this.loadAlarms()\n    await this.loadCategories()\n  },\n  methods: {\n    async loadAlarms() {\n      this.loading = true\n      try {\n        const response = await alarmApi.getAlarms({\n          page: this.currentPage,\n          page_size: this.pageSize\n        })\n        \n        // 转换数据格式，确保前端显示正确\n        this.alarms = response.results.map(alarm => ({\n          id: alarm.id,\n          created_at: alarm.created_at,\n          category_name: alarm.category ? alarm.category.name : '未分类',\n          category_id: alarm.category ? alarm.category.id : null,\n          latitude: alarm.latitude,\n          longitude: alarm.longitude,\n          content: alarm.content,\n          image_url: alarm.image_url,\n          status: alarm.status\n        }))\n        \n        this.totalAlarms = response.count\n      } catch (error) {\n        this.$message.error('获取告警列表失败')\n        console.error('Load alarms error:', error)\n      } finally {\n        this.loading = false\n      }\n    },\n    \n    async loadCategories() {\n      try {\n        const categories = await alarmApi.getAlarmCategories()\n        this.categories = categories\n      } catch (error) {\n        this.$message.error('获取告警类型失败')\n        console.error('Load categories error:', error)\n      }\n    },\n    \n    formatDate(dateString) {\n      if (!dateString) return ''\n      const date = new Date(dateString)\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      })\n    },\n    \n    getStatusText(status) {\n      const statusMap = {\n        'PENDING': '待处理',\n        'PROCESSING': '处理中',\n        'COMPLETED': '已完成',\n        'IGNORED': '已忽略'\n      }\n      return statusMap[status] || status\n    },\n    \n    getStatusType(status) {\n      const typeMap = {\n        'PENDING': 'danger',\n        'PROCESSING': 'warning',\n        'COMPLETED': 'success',\n        'IGNORED': 'info'\n      }\n      return typeMap[status] || 'default'\n    },\n    \n    getCategoryType(categoryName) {\n      // 根据不同的告警类型返回不同的标签类型\n      const typeKeywords = {\n        '发热': 'danger',\n        '损伤': 'warning',\n        '故障': 'danger',\n        '异常': 'warning',\n        '隐患': 'warning'\n      }\n      \n      for (const [keyword, type] of Object.entries(typeKeywords)) {\n        if (categoryName.includes(keyword)) {\n          return type\n        }\n      }\n      \n      return 'info'\n    },\n    \n    openAddAlarmDialog() {\n      this.isEdit = false\n      this.dialogTitle = '新增告警'\n      this.resetForm()\n      this.showDialog = true\n    },\n    \n    resetForm() {\n      this.alarmForm = {\n        category: '',\n        latitude: '',\n        longitude: '',\n        content: '',\n        image_url: '',\n        status: 'PENDING'\n      }\n      \n      if (this.$refs.alarmForm) {\n        this.$refs.alarmForm.resetFields()\n      }\n    },\n    \n    async submitAlarm() {\n      this.$refs.alarmForm.validate(async (valid) => {\n        if (valid) {\n          try {\n            if (this.isEdit) {\n              // 更新告警\n              await alarmApi.updateAlarm(this.currentAlarmId, this.alarmForm)\n              this.$message.success('告警更新成功')\n            } else {\n              // 创建新告警\n              await alarmApi.createAlarm(this.alarmForm)\n              this.$message.success('告警创建成功')\n            }\n            \n            this.cancelDialog()\n            await this.loadAlarms() // 重新加载列表\n          } catch (error) {\n            this.$message.error(this.isEdit ? '更新失败' : '创建失败')\n            console.error('Submit alarm error:', error)\n          }\n        }\n      })\n    },\n    \n    cancelDialog() {\n      this.showDialog = false\n      this.resetForm()\n    },\n    \n    viewAlarmDetail(alarm) {\n      // 查看详情，可以打开一个新的详情对话框\n      this.$message.info(`查看告警ID: ${alarm.id} 的详情`)\n      // 这里可以实现更详细的查看功能\n    },\n    \n    editAlarm(alarm) {\n      this.isEdit = true\n      this.dialogTitle = '编辑告警'\n      this.currentAlarmId = alarm.id\n      \n      // 填充表单数据\n      this.alarmForm = {\n        category: alarm.category_id,\n        latitude: alarm.latitude,\n        longitude: alarm.longitude,\n        content: alarm.content,\n        image_url: alarm.image_url,\n        status: alarm.status\n      }\n      \n      this.showDialog = true\n    },\n    \n    async deleteAlarm(alarmId) {\n      this.$confirm('确定要删除这条告警吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        try {\n          await alarmApi.deleteAlarm(alarmId)\n          this.$message.success('删除成功')\n          await this.loadAlarms() // 重新加载列表\n        } catch (error) {\n          this.$message.error('删除失败')\n          console.error('Delete alarm error:', error)\n        }\n      }).catch(() => {\n        // 取消删除\n      })\n    },\n    \n    handleSizeChange(size) {\n      this.pageSize = size\n      this.loadAlarms()\n    },\n    \n    handleCurrentChange(current) {\n      this.currentPage = current\n      this.loadAlarms()\n    }\n  }\n}\n</script>\n\n<style scoped>\n.alarm-list-container {\n  padding: 20px;\n}\n\n.header-actions {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.title {\n  font-size: 20px;\n  font-weight: bold;\n  margin: 0;\n}\n\n.search-filters {\n