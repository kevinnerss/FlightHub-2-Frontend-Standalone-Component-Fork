              <div class=\"loading-spinner small\"></div>\n            </div>\n            <AlarmPanel \n              v-else\n              :alarms=\"getFilteredAlarms()\"\n              @refresh=\"handleAlarmRefresh\"\n              @view-detail=\"handleViewAlarmDetail\"\n              @process-alarm=\"handleProcessAlarm\"\n              class=\"alarm-panel-component\"\n            />\n          </div>\n        </div>\n        \n        <!-- ÂÆûÊó∂ÁõëÊéß -->\n        <div class=\"panel-card\">\n          <div class=\"panel-header\">\n            <span class=\"panel-icon\">üìπ</span>\n            <h3 class=\"panel-title\">ÂÆûÊó∂ÁõëÊéß</h3>\n          </div>\n          <div class=\"panel-body placeholder-body\">\n            <p class=\"placeholder-text\">ÂÆûÊó∂ÁõëÊéßÁîªÈù¢</p>\n            <span class=\"placeholder-sub\">ÔºàÂ§ßÁñÜÁªÑ‰ª∂Êé•ÂÖ•Âå∫ÂüüÔºâ</span>\n          </div>\n        </div>\n          \n        <!-- Êó†‰∫∫Êú∫ÊéßÂà∂ -->\n        <div class=\"panel-card\">\n          <div class=\"panel-header\">\n            <span class=\"panel-icon\">üéÆ</span>\n            <h3 class=\"panel-title\">È£ûË°åÊéßÂà∂</h3>\n          </div>\n          <div class=\"panel-body placeholder-body\">\n            <p class=\"placeholder-text\">ÊéßÂà∂Èù¢Êùø</p>\n            <span class=\"placeholder-sub\">ÔºàÂ§ßÁñÜÁªÑ‰ª∂Êé•ÂÖ•Âå∫ÂüüÔºâ</span>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n    <!-- Ëà™Á∫øÂàõÂª∫ÂØπËØùÊ°Ü -->\n    <el-dialog\n      v-model=\"showWaylineCreation\"\n      title=\"ÂàõÂª∫Ëà™Á∫ø\"\n      width=\"90%\"\n      destroy-on-close\n      class=\"premium-dialog\"\n    >\n      <div style=\"height: 70vh\">\n        <DjiWaylineCreation \n          v-if=\"showWaylineCreation\"\n          ref=\"waylineCreationComponent\"\n          :config=\"djiConfig\"\n        />\n      </div>\n    </el-dialog>\n    \n    <!-- ÂëäË≠¶ËØ¶ÊÉÖÂºπÁ™ó -->\n    <el-dialog\n      v-model=\"showAlarmDetail\"\n      title=\"ÂëäË≠¶ËØ¶ÊÉÖ\"\n      width=\"800px\"\n      :close-on-click-modal=\"false\"\n      class=\"premium-dialog\"\n    >\n      <div v-if=\"currentAlarm\" class=\"alarm-detail-premium\">\n        <div class=\"detail-grid\">\n          <div class=\"detail-item\">\n            <label>ÂëäË≠¶ID</label>\n            <div class=\"value\">{{ currentAlarm.id }}</div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Á±ªÂûã</label>\n            <div class=\"value\">{{ currentAlarm.category_details?.name || 'Êú™ÂàÜÁ±ª' }}</div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Áä∂ÊÄÅ</label>\n            <div class=\"value\">\n              <span :class=\"['status-tag', `status-${currentAlarm.status.toLowerCase()}`]\">\n                {{ getStatusText(currentAlarm.status) }}\n              </span>\n            </div>\n          </div>\n          <div class=\"detail-item\">\n            <label>Êó∂Èó¥</label>\n            <div class=\"value\">{{ currentAlarm.created_at ? new Date(currentAlarm.created_at).toLocaleString('zh-CN') : '--' }}</div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>‰ΩçÁΩÆ</label>\n            <div class=\"value font-mono\">\n              <span class=\"coord\">LAT: {{ currentAlarm.latitude || '--' }}</span>\n              <span class=\"coord\">LNG: {{ currentAlarm.longitude || '--' }}</span>\n            </div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>ÂÖ≥ËÅîËà™Á∫ø</label>\n            <div class=\"value\">{{ currentAlarm.wayline?.name || currentAlarm.wayline_details?.name || 'Êú™Áü•Ëà™Á∫ø' }}</div>\n          </div>\n          <div class=\"detail-item full-width\">\n            <label>ÊèèËø∞</label>\n            <div class=\"value description-box\">{{ currentAlarm.content || '--' }}</div>\n          </div>\n          \n          <div v-if=\"currentAlarm.image_url\" class=\"detail-item full-width\">\n            <label>Áé∞Âú∫ÂõæÁâá</label>\n            <div clas