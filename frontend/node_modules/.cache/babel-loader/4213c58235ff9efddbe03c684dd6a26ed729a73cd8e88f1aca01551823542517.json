{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { ref, computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'UserManagement',\n  setup() {\n    const store = useStore();\n    const userFormRef = ref(null);\n    onMounted(() => {\n      loadUsers();\n    });\n\n    // ÂìçÂ∫îÂºèÊï∞ÊçÆ\n    const searchQuery = ref('');\n    const dialogVisible = ref(false);\n    const deleteDialogVisible = ref(false);\n    const editingUserId = ref(null);\n    const selectedUserName = ref('');\n    const selectedUserId = ref(null);\n    const isSubmitting = ref(false);\n    const isDeleting = ref(false);\n    const pageSizeLocal = ref(10);\n    const formData = ref({\n      username: '',\n      name: '',\n      password: '',\n      role: 'user'\n    });\n\n    // ËÆ°ÁÆóÂ±ûÊÄß\n    const allUsers = computed(() => store.getters.allUsers);\n    const currentPage = computed(() => store.state.currentPage);\n    const pageSize = computed(() => store.state.pageSize);\n    const totalUsers = computed(() => store.state.totalUsers);\n    const dialogTitle = computed(() => {\n      return editingUserId.value ? 'ÁºñËæëÁî®Êà∑' : 'Ê∑ªÂä†Áî®Êà∑';\n    });\n\n    // Âä†ËΩΩÁî®Êà∑ÂàóË°®\n    const loadUsers = async () => {\n      try {\n        await store.dispatch('fetchUsers', {\n          page: currentPage.value,\n          page_size: pageSize.value,\n          search: searchQuery.value\n        });\n      } catch (error) {\n        console.error('Âä†ËΩΩÁî®Êà∑Â§±Ë¥•:', error);\n        ElMessage.error('Âä†ËΩΩÁî®Êà∑ÂàóË°®Â§±Ë¥•');\n      }\n    };\n\n    // ÂàÜÈ°µ‰∫ã‰ª∂Â§ÑÁêÜ\n    const handlePageChange = page => {\n      if (page < 1 || page > Math.ceil(totalUsers.value / pageSize.value)) return;\n      store.commit('SET_PAGINATION_INFO', {\n        currentPage: page\n      });\n      loadUsers();\n    };\n    const handleSizeChange = () => {\n      store.commit('SET_PAGINATION_INFO', {\n        pageSize: pageSizeLocal.value,\n        currentPage: 1\n      });\n      loadUsers();\n    };\n\n    // ÊêúÁ¥¢ÂäüËÉΩ - ‰ΩøÁî®Èò≤Êäñ\n    let searchTimeout = null;\n    const handleSearch = () => {\n      if (searchTimeout) clearTimeout(searchTimeout);\n      searchTimeout = setTimeout(() => {\n        // ÊêúÁ¥¢Êó∂ÈáçÁΩÆÂà∞Á¨¨‰∏ÄÈ°µ\n        store.commit('SET_PAGINATION_INFO', {\n          currentPage: 1\n        });\n        loadUsers();\n      }, 300);\n    };\n\n    // ÊñπÊ≥ï\n    const formatDate = dateString => {\n      const date = new Date(dateString);\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n    const showAddUserDialog = () => {\n      resetForm();\n      editingUserId.value = null;\n      dialogVisible.value = true;\n    };\n    const showEditUserDialog = user => {\n      editingUserId.value = user.id;\n      formData.value = {\n        username: user.username,\n        name: user.name,\n        password: '',\n        role: user.role\n      };\n      dialogVisible.value = true;\n    };\n    const confirmDeleteUser = user => {\n      selectedUserId.value = user.id;\n      selectedUserName.value = user.name;\n      deleteDialogVisible.value = true;\n    };\n    const closeDialog = () => {\n      dialogVisible.value = false;\n      resetForm();\n    };\n    const resetForm = () => {\n      formData.value = {\n        username: '',\n        name: '',\n        password: '',\n        role: 'user'\n      };\n      editingUserId.value = null;\n    };\n    const submitForm = async () => {\n      try {\n        // ÁÆÄÂçïÈ™åËØÅ\n        if (!formData.value.username || !formData.value.name) {\n          ElMessage.error('ËØ∑Â°´ÂÜôÂÆåÊï¥‰ø°ÊÅØ');\n          return;\n        }\n        if (editingUserId.value === null && !formData.value.password) {\n          ElMessage.error('ËØ∑ËæìÂÖ•ÂØÜÁ†Å');\n          return;\n        }\n        isSubmitting.value = true;\n        if (editingUserId.value) {\n          // ÁºñËæëÁî®Êà∑\n          const form = {\n            ...formData.value\n          };\n          if (!form.password) {\n            delete form.password;\n          }\n          await store.dispatch('updateUser', {\n            id: editingUserId.value,\n            ...form\n          });\n          ElMessage.success('Áî®Êà∑ÁºñËæëÊàêÂäü');\n          await loadUsers();\n        } else {\n          // Ê∑ªÂä†Áî®Êà∑\n          const existingUser = allUsers.value.find(user => user.username === formData.value.username);\n          if (existingUser) {\n            ElMessage.error('Ë¥¶Âè∑Â∑≤Â≠òÂú®ÔºåËØ∑ÈÄâÊã©ÂÖ∂‰ªñË¥¶Âè∑');\n            return;\n          }\n          await store.dispatch('addUser', formData.value);\n          ElMessage.success('Áî®Êà∑Ê∑ªÂä†ÊàêÂäü');\n          await loadUsers();\n        }\n        dialogVisible.value = false;\n        resetForm();\n      } catch (error) {\n        console.error('Êìç‰ΩúÂ§±Ë¥•:', error);\n        ElMessage.error(error.message || 'Êìç‰ΩúÂ§±Ë¥•ÔºåËØ∑ÈáçËØï');\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n    const deleteUser = async () => {\n      try {\n        isDeleting.value = true;\n        const user = allUsers.value.find(u => u.id === selectedUserId.value);\n        if (user && user.username === 'admin') {\n          ElMessage.error('‰∏çËÉΩÂà†Èô§Á≥ªÁªüÁÆ°ÁêÜÂëòÁî®Êà∑');\n          return;\n        }\n        await store.dispatch('deleteUser', selectedUserId.value);\n        ElMessage.success('Áî®Êà∑Âà†Èô§ÊàêÂäü');\n        deleteDialogVisible.value = false;\n        await loadUsers();\n      } catch (error) {\n        console.error('Âà†Èô§Â§±Ë¥•:', error);\n        ElMessage.error(error.message || 'Âà†Èô§Â§±Ë¥•ÔºåËØ∑ÈáçËØï');\n      } finally {\n        isDeleting.value = false;\n      }\n    };\n    return {\n      searchQuery,\n      dialogVisible,\n      deleteDialogVisible,\n      selectedUserName,\n      selectedUserId,\n      isSubmitting,\n      isDeleting,\n      formData,\n      allUsers,\n      currentPage,\n      pageSize,\n      pageSizeLocal,\n      totalUsers,\n      dialogTitle,\n      editingUserId,\n      userFormRef,\n      formatDate,\n      showAddUserDialog,\n      showEditUserDialog,\n      confirmDeleteUser,\n      closeDialog,\n      submitForm,\n      deleteUser,\n      handlePageChange,\n      handleSizeChange,\n      handleSearch\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useStore","ElMessage","name","setup","store","userFormRef","loadUsers","searchQuery","dialogVisible","deleteDialogVisible","editingUserId","selectedUserName","selectedUserId","isSubmitting","isDeleting","pageSizeLocal","formData","username","password","role","allUsers","getters","currentPage","state","pageSize","totalUsers","dialogTitle","value","dispatch","page","page_size","search","error","console","handlePageChange","Math","ceil","commit","handleSizeChange","searchTimeout","handleSearch","clearTimeout","setTimeout","formatDate","dateString","date","Date","toLocaleString","year","month","day","hour","minute","showAddUserDialog","resetForm","showEditUserDialog","user","id","confirmDeleteUser","closeDialog","submitForm","form","success","existingUser","find","message","deleteUser","u"],"sources":["D:\\projects\\FlightHub-2-Frontend-Standalone-Component\\frontend\\src\\views\\UserManagement.vue"],"sourcesContent":["<template>\n  <div class=\"user-management-premium\">\n    <!-- È°µÈù¢Â§¥ÈÉ® -->\n    <div class=\"page-header-premium\">\n      <div class=\"header-content\">\n        <div class=\"header-left\">\n          <div class=\"header-icon\">\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n              <path d=\"M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n              <circle cx=\"9\" cy=\"7\" r=\"4\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n              <path d=\"M23 21v-2a4 4 0 0 0-3-3.87M16 3.13a4 4 0 0 1 0 7.75\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n            </svg>\n          </div>\n          <div class=\"header-text\">\n            <h1 class=\"page-title\">‰∫∫ÂëòÁÆ°ÁêÜ</h1>\n            <p class=\"page-subtitle\">ÁÆ°ÁêÜÁ≥ªÁªüÁî®Êà∑Ë¥¶Âè∑ÂíåÊùÉÈôê</p>\n          </div>\n        </div>\n        <button @click=\"showAddUserDialog\" class=\"add-user-btn-premium\">\n          <span class=\"btn-icon\">+</span>\n          <span>Ê∑ªÂä†Áî®Êà∑</span>\n        </button>\n      </div>\n    </div>\n\n    <!-- ÊêúÁ¥¢Á≠õÈÄâÂå∫ -->\n    <div class=\"search-section-premium\">\n      <div class=\"search-wrapper\">\n        <div class=\"search-icon\">üîç</div>\n        <input \n          v-model=\"searchQuery\"\n          @input=\"handleSearch\"\n          placeholder=\"ÊêúÁ¥¢Ë¥¶Âè∑ÊàñÂßìÂêç...\"\n          class=\"search-input-premium\"\n        />\n      </div>\n      <div class=\"stats-cards\">\n        <div class=\"stat-card\">\n          <div class=\"stat-label\">ÊÄªÁî®Êà∑Êï∞</div>\n          <div class=\"stat-value\">{{ totalUsers }}</div>\n        </div>\n        <div class=\"stat-card\">\n          <div class=\"stat-label\">ÂΩìÂâçÈ°µ</div>\n          <div class=\"stat-value\">{{ currentPage }}/{{ Math.ceil(totalUsers / pageSize) || 1 }}</div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Áî®Êà∑ÂàóË°®Âç°Áâá -->\n    <div class=\"table-card-premium\">\n      <div class=\"table-wrapper-premium\">\n        <table class=\"premium-table\">\n          <thead>\n            <tr>\n              <th width=\"80\">ID</th>\n              <th width=\"180\">Ë¥¶Âè∑</th>\n              <th width=\"180\">ÂßìÂêç</th>\n              <th width=\"120\">ËßíËâ≤</th>\n              <th width=\"220\">ÂàõÂª∫Êó∂Èó¥</th>\n              <th width=\"180\">Êìç‰Ωú</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"user in allUsers\" :key=\"user.id\" class=\"table-row\">\n              <td>\n                <span class=\"id-badge\">{{ user.id }}</span>\n              </td>\n              <td>\n                <div class=\"username-cell\">\n                  <div class=\"user-avatar-small\">{{ user.username.charAt(0).toUpperCase() }}</div>\n                  <span>{{ user.username }}</span>\n                </div>\n              </td>\n              <td>{{ user.name }}</td>\n              <td>\n                <span class=\"role-badge\" :class=\"user.role === 'admin' ? 'role-admin' : 'role-user'\">\n                  {{ user.role === 'admin' ? 'ÁÆ°ÁêÜÂëò' : 'ÊôÆÈÄöÁî®Êà∑' }}\n                </span>\n              </td>\n              <td>\n                <span class=\"datetime-text\">{{ formatDate(user.createdAt) }}</span>\n              </td>\n              <td>\n                <div v-if=\"user.username !== 'admin'\" class=\"action-buttons\">\n                  <button @click=\"showEditUserDialog(user)\" class=\"action-btn edit-btn\">\n                    <span>ÁºñËæë</span>\n                  </button>\n                  <button @click=\"confirmDeleteUser(user)\" class=\"action-btn delete-btn\">\n                    <span>Âà†Èô§</span>\n                  </button>\n                </div>\n                <div v-else class=\"system-admin-badge\">\n                  Á≥ªÁªüÁÆ°ÁêÜÂëò\n                </div>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n\n      <!-- ÂàÜÈ°µÂô® -->\n      <div class=\"pagination-premium\">\n        <div class=\"pagination-info\">\n          ÊòæÁ§∫ {{ Math.min((currentPage - 1) * pageSize + 1, totalUsers) }} - {{ Math.min(currentPage * pageSize, totalUsers) }} Êù°ÔºåÂÖ± {{ totalUsers }} Êù°\n        </div>\n        <div class=\"pagination-controls\">\n          <button @click=\"handlePageChange(currentPage - 1)\" :disabled=\"currentPage === 1\" class=\"pagination-btn\">\n            <span>‚Äπ</span>\n          </button>\n          <input \n            type=\"number\" \n            :value=\"currentPage\"\n            @change=\"handlePageChange(Number($event.target.value))\"\n            class=\"page-input\"\n            min=\"1\"\n            :max=\"Math.ceil(totalUsers / pageSize)\"\n          />\n          <span class=\"pagination-separator\">/</span>\n          <span class=\"total-pages\">{{ Math.ceil(totalUsers / pageSize) || 1 }}</span>\n          <button @click=\"handlePageChange(currentPage + 1)\" :disabled=\"currentPage >= Math.ceil(totalUsers / pageSize)\" class=\"pagination-btn\">\n            <span>‚Ä∫</span>\n          </button>\n          <select v-model.number=\"pageSizeLocal\" @change=\"handleSizeChange\" class=\"page-size-select\">\n            <option :value=\"5\">5 Êù°/È°µ</option>\n            <option :value=\"10\">10 Êù°/È°µ</option>\n            <option :value=\"20\">20 Êù°/È°µ</option>\n            <option :value=\"50\">50 Êù°/È°µ</option>\n          </select>\n        </div>\n      </div>\n    </div>\n\n    <!-- Ê∑ªÂä†/ÁºñËæëÁî®Êà∑ÂØπËØùÊ°Ü -->\n    <div v-if=\"dialogVisible\" class=\"modal-overlay\" @click.self=\"closeDialog\">\n      <div class=\"modal-premium\">\n        <div class=\"modal-header\">\n          <h2 class=\"modal-title\">{{ dialogTitle }}</h2>\n          <button @click=\"closeDialog\" class=\"modal-close\">√ó</button>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"form-group\">\n            <label class=\"form-label\">Ë¥¶Âè∑</label>\n            <input \n              v-model=\"formData.username\"\n              :disabled=\"editingUserId !== null\"\n              placeholder=\"ËØ∑ËæìÂÖ•Ë¥¶Âè∑\"\n              class=\"form-input\"\n            />\n          </div>\n          <div class=\"form-group\">\n            <label class=\"form-label\">ÂßìÂêç</label>\n            <input \n              v-model=\"formData.name\"\n              placeholder=\"ËØ∑ËæìÂÖ•ÂßìÂêç\"\n              class=\"form-input\"\n            />\n          </div>\n          <div class=\"form-group\">\n            <label class=\"form-label\">ÂØÜÁ†Å{{ editingUserId !== null ? 'Ôºà‰∏ç‰øÆÊîπËØ∑ÁïôÁ©∫Ôºâ' : '' }}</label>\n            <input \n              v-model=\"formData.password\"\n              type=\"password\"\n              placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\"\n              class=\"form-input\"\n            />\n          </div>\n          <div class=\"form-group\">\n            <label class=\"form-label\">ËßíËâ≤</label>\n            <select v-model=\"formData.role\" class=\"form-select\">\n              <option value=\"user\">ÊôÆÈÄöÁî®Êà∑</option>\n              <option value=\"admin\">ÁÆ°ÁêÜÂëò</option>\n            </select>\n          </div>\n        </div>\n        <div class=\"modal-footer\">\n          <button @click=\"closeDialog\" class=\"modal-btn secondary-btn\">ÂèñÊ∂à</button>\n          <button @click=\"submitForm\" :disabled=\"isSubmitting\" class=\"modal-btn primary-btn\">\n            {{ isSubmitting ? 'Êèê‰∫§‰∏≠...' : 'Á°ÆÂÆö' }}\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <!-- Âà†Èô§Á°ÆËÆ§ÂØπËØùÊ°Ü -->\n    <div v-if=\"deleteDialogVisible\" class=\"modal-overlay\" @click.self=\"deleteDialogVisible = false\">\n      <div class=\"modal-premium delete-modal\">\n        <div class=\"modal-header\">\n          <h2 class=\"modal-title\">Á°ÆËÆ§Âà†Èô§</h2>\n          <button @click=\"deleteDialogVisible = false\" class=\"modal-close\">√ó</button>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"delete-warning\">\n            <div class=\"warning-icon\">‚ö†Ô∏è</div>\n            <p>Á°ÆÂÆöË¶ÅÂà†Èô§Áî®Êà∑ <strong>{{ selectedUserName }}</strong> ÂêóÔºü</p>\n            <p class=\"warning-text\">Ê≠§Êìç‰Ωú‰∏çÂèØÊí§ÈîÄÔºåËØ∑Ë∞®ÊÖéÊìç‰ΩúÔºÅ</p>\n          </div>\n        </div>\n        <div class=\"modal-footer\">\n          <button @click=\"deleteDialogVisible = false\" class=\"modal-btn secondary-btn\">ÂèñÊ∂à</button>\n          <button @click=\"deleteUser\" :disabled=\"isDeleting\" class=\"modal-btn danger-btn\">\n            {{ isDeleting ? 'Âà†Èô§‰∏≠...' : 'Á°ÆËÆ§Âà†Èô§' }}\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue'\nimport { useStore } from 'vuex'\nimport { ElMessage } from 'element-plus'\n\nexport default {\n  name: 'UserManagement',\n  setup() {\n    const store = useStore()\n    const userFormRef = ref(null)\n    \n    onMounted(() => {\n      loadUsers()\n    })\n    \n    // ÂìçÂ∫îÂºèÊï∞ÊçÆ\n    const searchQuery = ref('')\n    const dialogVisible = ref(false)\n    const deleteDialogVisible = ref(false)\n    const editingUserId = ref(null)\n    const selectedUserName = ref('')\n    const selectedUserId = ref(null)\n    const isSubmitting = ref(false)\n    const isDeleting = ref(false)\n    const pageSizeLocal = ref(10)\n    \n    const formData = ref({\n      username: '',\n      name: '',\n      password: '',\n      role: 'user'\n    })\n    \n    // ËÆ°ÁÆóÂ±ûÊÄß\n    const allUsers = computed(() => store.getters.allUsers)\n    const currentPage = computed(() => store.state.currentPage)\n    const pageSize = computed(() => store.state.pageSize)\n    const totalUsers = computed(() => store.state.totalUsers)\n    \n    const dialogTitle = computed(() => {\n      return editingUserId.value ? 'ÁºñËæëÁî®Êà∑' : 'Ê∑ªÂä†Áî®Êà∑'\n    })\n    \n    // Âä†ËΩΩÁî®Êà∑ÂàóË°®\n    const loadUsers = async () => {\n      try {\n        await store.dispatch('fetchUsers', {\n          page: currentPage.value,\n          page_size: pageSize.value,\n          search: searchQuery.value\n        })\n      } catch (error) {\n        console.error('Âä†ËΩΩÁî®Êà∑Â§±Ë¥•:', error)\n        ElMessage.error('Âä†ËΩΩÁî®Êà∑ÂàóË°®Â§±Ë¥•')\n      }\n    }\n    \n    // ÂàÜÈ°µ‰∫ã‰ª∂Â§ÑÁêÜ\n    const handlePageChange = (page) => {\n      if (page < 1 || page > Math.ceil(totalUsers.value / pageSize.value)) return\n      store.commit('SET_PAGINATION_INFO', { currentPage: page })\n      loadUsers()\n    }\n    \n    const handleSizeChange = () => {\n      store.commit('SET_PAGINATION_INFO', { pageSize: pageSizeLocal.value, currentPage: 1 })\n      loadUsers()\n    }\n    \n    // ÊêúÁ¥¢ÂäüËÉΩ - ‰ΩøÁî®Èò≤Êäñ\n    let searchTimeout = null\n    const handleSearch = () => {\n      if (searchTimeout) clearTimeout(searchTimeout)\n      searchTimeout = setTimeout(() => {\n        // ÊêúÁ¥¢Êó∂ÈáçÁΩÆÂà∞Á¨¨‰∏ÄÈ°µ\n        store.commit('SET_PAGINATION_INFO', { currentPage: 1 })\n        loadUsers()\n      }, 300)\n    }\n    \n    // ÊñπÊ≥ï\n    const formatDate = (dateString) => {\n      const date = new Date(dateString)\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      })\n    }\n    \n    const showAddUserDialog = () => {\n      resetForm()\n      editingUserId.value = null\n      dialogVisible.value = true\n    }\n    \n    const showEditUserDialog = (user) => {\n      editingUserId.value = user.id\n      formData.value = {\n        username: user.username,\n        name: user.name,\n        password: '',\n        role: user.role\n      }\n      dialogVisible.value = true\n    }\n    \n    const confirmDeleteUser = (user) => {\n      selectedUserId.value = user.id\n      selectedUserName.value = user.name\n      deleteDialogVisible.value = true\n    }\n    \n    const closeDialog = () => {\n      dialogVisible.value = false\n      resetForm()\n    }\n    \n    const resetForm = () => {\n      formData.value = {\n        username: '',\n        name: '',\n        password: '',\n        role: 'user'\n      }\n      editingUserId.value = null\n    }\n    \n    const submitForm = async () => {\n      try {\n        // ÁÆÄÂçïÈ™åËØÅ\n        if (!formData.value.username || !formData.value.name) {\n          ElMessage.error('ËØ∑Â°´ÂÜôÂÆåÊï¥‰ø°ÊÅØ')\n          return\n        }\n        \n        if (editingUserId.value === null && !formData.value.password) {\n          ElMessage.error('ËØ∑ËæìÂÖ•ÂØÜÁ†Å')\n          return\n        }\n        \n        isSubmitting.value = true\n        \n        if (editingUserId.value) {\n          // ÁºñËæëÁî®Êà∑\n          const form = { ...formData.value }\n          if (!form.password) {\n            delete form.password\n          }\n          \n          await store.dispatch('updateUser', { id: editingUserId.value, ...form })\n          ElMessage.success('Áî®Êà∑ÁºñËæëÊàêÂäü')\n          await loadUsers()\n        } else {\n          // Ê∑ªÂä†Áî®Êà∑\n          const existingUser = allUsers.value.find(\n            user => user.username === formData.value.username\n          )\n          \n          if (existingUser) {\n            ElMessage.error('Ë¥¶Âè∑Â∑≤Â≠òÂú®ÔºåËØ∑ÈÄâÊã©ÂÖ∂‰ªñË¥¶Âè∑')\n            return\n          }\n          \n          await store.dispatch('addUser', formData.value)\n          ElMessage.success('Áî®Êà∑Ê∑ªÂä†ÊàêÂäü')\n          await loadUsers()\n        }\n        \n        dialogVisible.value = false\n        resetForm()\n      } catch (error) {\n        console.error('Êìç‰ΩúÂ§±Ë¥•:', error)\n        ElMessage.error(error.message || 'Êìç‰ΩúÂ§±Ë¥•ÔºåËØ∑ÈáçËØï')\n      } finally {\n        isSubmitting.value = false\n      }\n    }\n    \n    const deleteUser = async () => {\n      try {\n        isDeleting.value = true\n        \n        const user = allUsers.value.find(u => u.id === selectedUserId.value)\n        if (user && user.username === 'admin') {\n          ElMessage.error('‰∏çËÉΩÂà†Èô§Á≥ªÁªüÁÆ°ÁêÜÂëòÁî®Êà∑')\n          return\n        }\n        \n        await store.dispatch('deleteUser', selectedUserId.value)\n        ElMessage.success('Áî®Êà∑Âà†Èô§ÊàêÂäü')\n        deleteDialogVisible.value = false\n        