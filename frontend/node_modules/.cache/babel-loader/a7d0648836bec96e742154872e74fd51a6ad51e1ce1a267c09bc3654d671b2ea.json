    <div class=\"header-row\">\n              <span :class=\"['status-tag', `status-${alarm.status.toLowerCase()}`]\">\n                {{ getStatusText(alarm.status) }}\n              </span>\n              <span class=\"time\">{{ formatTime(alarm.created_at) }}</span>\n            </div>\n            <h4 class=\"alarm-title\">{{ getAlarmTitle(alarm) }}</h4>\n          </div>\n          \n          <div class=\"card-body\">\n            <p class=\"content\">{{ alarm.content }}</p>\n            <div class=\"location\" v-if=\"alarm.latitude\">\n              <i class=\"el-icon-location-outline\"></i>\n              <span>{{ alarm.latitude }}, {{ alarm.longitude }}</span>\n            </div>\n          </div>\n\n          <div class=\"card-footer\">\n            <button class=\"action-btn view\" @click=\"$emit('view-detail', alarm)\">详情</button>\n            <button class=\"action-btn process\" @click=\"$emit('process-alarm', alarm.id)\">处理</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AlarmPanel',\n  props: {\n    alarms: {\n      type: Array,\n      default: () => []\n    }\n  },\n  methods: {\n    formatTime(timestamp) {\n      if (!timestamp) return ''\n      const date = new Date(timestamp)\n      return date.toLocaleTimeString('zh-CN', { hour: '2-digit', minute: '2-digit' })\n    },\n    getAlarmTitle(alarm) {\n      return alarm.category_details?.name || alarm.category_name || '系统告警'\n    },\n    getStatusText(status) {\n      const map = {\n        'PENDING': '待处理',\n        'PROCESSING': '处理中',\n        'COMPLETED': '已完成',\n        'IGNORED': '已忽略'\n      }\n      return map[status] || status\n    },\n    getPriority(alarm) {\n      // 简单的优先级判断逻辑\n      if (alarm.status === 'PENDING') return 'high'\n      if (alarm.status === 'PROCESSING') return 'medium'\n      return 'low'\n    }\n  }\n}\n</script>\n\n<style scoped>\n.alarm-panel-premium {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  background: transparent;\n}\n\n.panel-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 4px 12px 4px;\n  border-bottom: 1px solid rgba(0, 212, 255, 0.1);\n  margin-bottom: 12px;\n}\n\n.header-left {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.pulse-dot {\n  width: 8px;\n  height: 8px;\n  background: #ef4444;\n  border-radius: 50%;\n  box-shadow: 0 0 8px #ef4444;\n  animation: pulse 2s infinite;\n}\n\n@keyframes pulse {\n  0% { transform: scale(1); opacity: 1; }\n  50% { transform: scale(1.5); opacity: 0.5; }\n  100% { transform: scale(1); opacity: 1; }\n}\n\n.title {\n  font-size: 14px;\n  color: #e2e8f0;\n  margin: 0;\n  font-weight: 600;\n}\n\n.header-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.count-badge {\n  background: rgba(239, 68, 68, 0.2);\n  color: #ef4444;\n  padding: 2px 6px;\n  border-radius: 10px;\n  font-size: 12px;\n  font-weight: 600;\n  border: 1px solid rgba(239, 68, 68, 0.3);\n}\n\n.icon-btn {\n  background: transparent;\n  border: none;\n  color: #94a3b8;\n  cursor: pointer;\n  padding: 4px;\n  transition: color 0.2s;\n}\n\n.icon-btn:hover {\n  color: #00d4ff;\n}\n\n.alarm-list-scroll {\n  flex: 1;\n  overflow-y: auto;\n  padding-right: 4px;\n}\n\n/* 滚动条样式 */\n.alarm-list-scroll::-webkit-scrollbar {\n  width: 4px;\n}\n\n.alarm-list-scroll::-webkit-scrollbar-track {\n  background: rgba(0, 0, 0, 0.1);\n}\n\n.alarm-list-scroll::-webkit-scrollbar-thumb {\n  background: rgba(0, 212, 255, 0.2);\n  border-radius: 2px;\n}\n\n.empty-state {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  color: #64748b;\n  gap: 8px;\n  font-size: 13px;\n}\n\n.alarm-items {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.alarm-card {\n  background: rgba(15, 23, 42, 0.6);\n  border: 1px solid rgba(148, 163, 184, 0.1);\n  border-radius: 8px;\n  padding: 12px;\n  transition: all 0.2s;\n}\n\n.alarm-card:hover {\n  background: rgba(15, 23, 42, 0.8);\n  border-color: rgba(0, 212, 255, 0.3);\n  transform: translateY(-1px);\n}\n\n.alarm-card.priority-high {\n  border-left: 3px solid #ef4444;\n}\n\n.alarm-card.priority-medium {\n  border-left: 3px solid #f59e0b;\n}\n\n.alarm-card.priority-low {\n  border-left: 3px solid #10b981;\n}\n\n.card-header {\n  margin-bottom: 8px;\n}\n\n.header-row {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 4px;\n}\n\n.status-tag {\n  font-size: 10px;\n  padding: 2px 6px;\n  border-radius: 4px;\n}\n\n.status-pending { background: rgba(239, 68, 68, 0.15); color: #ef4444; }\n.status-processing { background: rgba(245, 158, 11, 0.15); color: #f59e0b; }\n.status-completed { background: rgba(16, 185, 129, 0.15); color: #10b981; }\n\n.time {\n  font-size: 11px;\n  color: #64748b;\n}\n\n.alarm-title {\n  margin: 0;\n  font-size: 13px;\n  color: #e2e8f0;\n  font-weight: 500;\n}\n\n.content {\n  font-size: 12px;\n  color: #94a3b8;\n  margin: 0 0 8px 0;\n  line-height: 1.4;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n  overflow: hidden;\n}\n\n.location {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  font-size: 11px;\n  color: #64748b;\n}\n\n.card-footer {\n  margin-top: 12px;\n  display: flex;\n  justify-content: flex-end;\n  gap: 8px;\n}\n\n.action-btn {\n  padding: 4px 12px;\n  border-radius: 4px;\n  font-size: 11px;\n  cursor: pointer;\n  border: none;\n  transition: all 0.2s;\n}\n\n.action-btn.view {\n  background: rgba(148, 163, 184, 0.1);\n  color: #94a3b8;\n}\n\n.action-btn.view:hover {\n  background: rgba(148, 163, 184, 0.2);\n  color: #e2e8f0;\n}\n\n.action-btn.process {\n  background: rgba(0, 212, 255, 0.1);\n  color: #00d4ff;\n}\n\n.action-btn.process:hover {\n  background: rgba(0, 212, 255, 0.2);\n  box-shadow: 0 0 8px rgba(0, 212, 255, 0.2);\n}\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAqB;;EACzBA,KAAK,EAAC;AAAc;;EAKlBA,